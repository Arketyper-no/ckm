definition
	CLUSTER[at0000] matches {	-- Examination of external auditory canal
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0097] occurrences matches {0..1} matches {	-- Ear examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0098, 	-- Left
							at0099]	-- Right
						}
					}
				}
			}
			ELEMENT[at0189] occurrences matches {0..1} matches {	-- No abnormality detected
				value matches {
					DV_BOOLEAN matches {
						value matches {True}
					}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0193] occurrences matches {0..1} matches {	-- Description of wax
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0106] occurrences matches {0..1} matches {	-- Consistency of wax
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0108, 	-- Soft
							at0107]	-- Hard
						}
					}
				}
			}
			ELEMENT[at0109] occurrences matches {0..1} matches {	-- State of wax
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
