						}
					}
				}
			}
			CLUSTER[at0064] occurrences matches {0..*} matches {	-- Specific finding
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0112] matches {	-- Finding
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
