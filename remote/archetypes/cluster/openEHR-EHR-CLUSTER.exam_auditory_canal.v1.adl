				}
			}
			ELEMENT[at0190] occurrences matches {0..1} matches {	-- Discharge presence
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0191, 	-- Present
							at0192]	-- Absent
						}
					}
				}
			}
			ELEMENT[at0157] occurrences matches {0..1} matches {	-- Discharge type
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0151] occurrences matches {0..1} matches {	-- Discharge amount
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0153, 	-- Scant
							at0154, 	-- Moderate
							at0155]	-- Profuse
						}
					}
				}
			}
			ELEMENT[at0156] occurrences matches {0..1} matches {	-- Discharge description
