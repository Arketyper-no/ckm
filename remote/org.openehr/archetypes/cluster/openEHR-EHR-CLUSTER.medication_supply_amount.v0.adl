				>
				["at0161"] = <
					text = <"Mengdebeskrivelse">
					description = <"En fritekstrepresentasjon av mengden legemiddel, vaksine eller terapeutisk vare som er ønsket levert eller faktisk levert.">
				>
			>
		>
		["pt-br"] = <
