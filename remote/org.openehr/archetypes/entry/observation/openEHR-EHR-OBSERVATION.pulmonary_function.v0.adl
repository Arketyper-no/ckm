definition
	OBSERVATION[at0000] occurrences matches {0..1} matches {    -- Pulmonary function test result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0052] occurrences matches {0..*} matches {    -- Volume
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0087] matches {    -- Test name
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0065,    -- Forced vital capacity (FVC)
															at0145,    -- FEV 0.5
															at0070,    -- FEV 0.75
															at0067,    -- FEV1
															at0146,    -- FEV2
															at0068,    -- FEV3
															at0069,    -- FEV6
															at0064,    -- Slow vital capacity (SVC/EVC)
															at0066,    -- Total lung capacity (TLC)
															at0082,    -- Tidal volume (TV/VT)
															at0077,    -- Expiratory reserve volume (ERV)
															at0078,    -- Inspiratory reserve volume (IRV)
															at0076,    -- Inspiratory capacity (IC)
															at0083,    -- Residual volume (RV)
															at0084,    -- Functional residual capacity (FRC)
															at0129,    -- Anatomical dead space
															at0150]    -- Vital capacity (VC)
														}
													}
												}
											}
											ELEMENT[at0053] occurrences matches {0..1} matches {    -- Result
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"l">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"ml">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {    -- Predicted result
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"l">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"ml">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0044] occurrences matches {0..1} matches {    -- Actual/predicted ratio
												value matches {
													DV_PROPORTION matches {
														numerator matches {|>=0.0|}
														denominator matches {|0.0..100.0|}
														type matches {0,2}
													}
												}
											}
											ELEMENT[at0125] occurrences matches {0..1} matches {    -- No test result
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0126] occurrences matches {0..1} matches {    -- Reason for no test result
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0057] occurrences matches {0..*} matches {    -- Flow rate
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0088] matches {    -- Test name
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0071,    -- Peak expiratory flow (PEF)
															at0152,    -- Forced expiratory flow (FEFmax)
															at0072,    -- Forced expiratory flow 25-75% (FEF25-75%)
															at0073,    -- Forced expiratory flow 25-50% (FEF25-50%)
															at0074,    -- Forced inspiratory flow 25-75% (FIF25-75%)
															at0075,    -- Forced inspratory flow 25-50% (FIF25-50%)
															at0079]    -- Maximum voluntary ventilation
														}
													}
												}
											}
											ELEMENT[at0058] occurrences matches {0..1} matches {    -- Result
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::126]>
														list = <
															["1"] = <
																units = <"l/s">
																magnitude = <|0.0..60000.0|>
																precision = <|0|>
															>
															["2"] = <
																units = <"ml/s">
																magnitude = <|0.0..200000.0|>
																precision = <|0|>
															>
															["3"] = <
																units = <"l/min">
																magnitude = <|0.0..1000.0|>
																precision = <|0|>
															>
														>
													>
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {    -- Predicted result
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::126]>
														list = <
															["1"] = <
																units = <"l/s">
																magnitude = <|0.0..60000.0|>
																precision = <|0|>
															>
															["2"] = <
																units = <"ml/s">
																magnitude = <|0.0..200000.0|>
																precision = <|0|>
															>
															["3"] = <
																units = <"l/min">
																magnitude = <|0.0..1000.0|>
																precision = <|0|>
															>
														>
													>
												}
											}
											ELEMENT[at0122] occurrences matches {0..1} matches {    -- Actual/predicted ratio
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														denominator matches {|0.0..100.0|}
														type matches {0,2}
													}
												}
											}
											ELEMENT[at0134] occurrences matches {0..1} matches {    -- No test result
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0135] occurrences matches {0..1} matches {    -- Reason for no test result
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0055] occurrences matches {0..*} matches {    -- Ratio
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0089] matches {    -- Ratio name
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0081,    -- FEV1/FVC ratio (FEV1%FVC)
															at0080]    -- Tiffenau index (FEV1/SVC)
														}
													}
												}
											}
											ELEMENT[at0056] occurrences matches {0..1} matches {    -- Result
												value matches {
													DV_PROPORTION matches {
														numerator matches {|>=0.0|}
														type matches {0,2}
													}
												}
											}
											ELEMENT[at0018] occurrences matches {0..1} matches {    -- Predicted result
												value matches {
													DV_PROPORTION matches {
														numerator matches {|>=0.0|}
														denominator matches {|>=0.0|}
														type matches {0,2}
													}
												}
											}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Forced expiratory time (FET)
										value matches {
											DV_DURATION matches {
												value matches {PTS/|PT0S..PT3S|}
											}
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- Diffusion lung capacity
										value matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"ml/min/mm[Hg]">
														magnitude = <|>=0.0|>
													>
													["2"] = <
														units = <"mmol/min/kPa">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									allow_archetype CLUSTER[at0131] occurrences matches {0..*} matches {    -- Multimedia representation
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0130] occurrences matches {0..1} matches {    -- Clinical interpretation
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0101] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0133] occurrences matches {0..*} matches {    -- Test not done
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0031] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0098] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0115] occurrences matches {0..1} matches {    -- Position
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0116,    -- Standing
													at0117,    -- Sitting
													at0118,    -- Reclining
													at0119]    -- Lying
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0029] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0030] occurrences matches {0..1} matches {    -- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0132] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0151] occurrences matches {0..*} matches {    -- Formula
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Pulmonary function test result">
					description = <"The results of a common suite of pulmonary function measurements and calculated ratios to assess lung volumes, airway function and gas exchange.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0008"] = <
					text = <"Predicted result">
					description = <"Predicted flow rate.">
				>
				["at0013"] = <
					text = <"Forced expiratory time (FET)">
					description = <"The time taken to complete a forced expiration.">
				>
				["at0018"] = <
					text = <"Predicted result">
					description = <"Predicted ratio.">
				>
				["at0029"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0030"] = <
					text = <"Device">
					description = <"Details of the device used to measure pulmonary function.">
				>
				["at0031"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0037"] = <
					text = <"Diffusion lung capacity">
					description = <"A test of the diffusion capacity (DLCO), or transfer factor (TLCO), of the lung for carbon monoxide (CO).">
					comment = <"Also called carbon monoxide diffusing capacity test.">
				>
				["at0044"] = <
					text = <"Actual/predicted ratio">
					description = <"The ratio of the measured to predicted test results.">
				>
				["at0052"] = <
					text = <"Volume">
					description = <"Test result represented as a measured volume or calculated capacity.">
				>
				["at0053"] = <
					text = <"Result">
					description = <"Measured volume.">
				>
				["at0054"] = <
					text = <"Predicted result">
					description = <"Predicted volume.">
				>
				["at0055"] = <
					text = <"Ratio">
					description = <"A calculated test result expressed as a ratio.">
				>
				["at0056"] = <
					text = <"Result">
					description = <"Calculated ratio.">
				>
				["at0057"] = <
					text = <"Flow rate">
					description = <"Test result represented as a measured flow rate.">
				>
				["at0058"] = <
					text = <"Result">
					description = <"Measured flow rate.">
				>
				["at0064"] = <
					text = <"Slow vital capacity (SVC/EVC)">
					description = <"The amount of air that can be forced out of the lungs after a maximal inspiration. Emphasis on completeness of expiration. Also termed expiratory vital capacity (EVC). Calculation = IRV+TV+ERV.">
				>
				["at0065"] = <
					text = <"Forced vital capacity (FVC)">
					description = <"The determination of the vital capacity from a maximally forced expiratory effort.">
				>
				["at0066"] = <
					text = <"Total lung capacity (TLC)">
					description = <"The volume in the lungs at maximal inflation. Calculation - VC+RV.">
				>
				["at0067"] = <
					text = <"FEV1">
					description = <"Volume that has been exhaled at the end of the first second of forced expiration.">
				>
				["at0068"] = <
					text = <"FEV3">
					description = <"Volume that has been exhaled at the end of the first three seconds of forced expiration.">
				>
				["at0069"] = <
					text = <"FEV6">
					description = <"Volume that has been exhaled at the end of the first six seconds of forced expiration.">
				>
				["at0070"] = <
					text = <"FEV 0.75">
					description = <"Volume that has been exhaled at the end of the first three quarters of a second of forced expiration.">
				>
				["at0071"] = <
					text = <"Peak expiratory flow (PEF)">
					description = <"The highest forced expiratory flow measured with a peak flow meter. Also termed Peak Expiratory Flow Rate (PEFR).">
				>
				["at0072"] = <
					text = <"Forced expiratory flow 25-75% (FEF25-75%)">
					description = <"The forced expiratory flow of air from the lung during the middle portion (25-75%) of expiration on the FVC curve.">
				>
				["at0073"] = <
					text = <"Forced expiratory flow 25-50% (FEF25-50%)">
					description = <"The forced expiratory flow of air from the lung during the middle portion (25-50%) of expiration on the FVC curve.">
				>
				["at0074"] = <
					text = <"Forced inspiratory flow 25-75% (FIF25-75%)">
