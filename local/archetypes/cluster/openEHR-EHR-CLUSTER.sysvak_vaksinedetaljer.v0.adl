	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Arketypen skal benyttes for bruk av registrering av vaksiner. 
">
			use = <"Arketypen skal brukes for å registere vaksinetype, om det er mangel av vaksine og vaksinasjonsdokumentasjon dersom det er etterregistrering.

Når vaksinand ikke har et fødselsnummer eller d-nummer tilgjengelig, skal det i denne arketypen registreres personopplysninger om vaksinanden, og ev. omsorgsperson.">
			keywords = <"sysvak", "vaksine", "vaksinering", "vaksinasjon">
			misuse = <"Arketypen skal ikke brukes for å registrere personopplysninger eller registrering av medisiner (preparatnavn og kode) utenfor området som omhandler vaksinering.">
			copyright = <"© Nasjonal IKT HF">
		>
	>
