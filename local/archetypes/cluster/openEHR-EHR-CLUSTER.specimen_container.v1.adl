				>
				["at0029"] = <
					text = <"Innhold">
					description = <"Innholdet i prøvebeholderen.">
					comment = <"For eksempel: sub-prøvebeholdere eller prøvemateriale.">
				>
				["at0035"] = <
					text = <"Kapasitet">
					description = <"Kapasiteten til prøvebeholderen.">
					comment = <"En fritekstbeskrivelse eller den spesifikke kapasiteten, f.eks. volum. For eksempel: \"125\" (125 mL), eller teksten \"Trekkvolum på 2 mL - 5 mL\", \"Indre mål er 12 cm i diameter og 9 cm høy\".">
				>
				["at0036"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om prøvematerialet som ikke dekkes av andre felt.">
				>
				["at0037"] = <
					text = <"Komponenter">
					description = <"Beskrivelse av de fysiske komponentene til prøvebeholderen.">
					comment = <"For eksempel: fargen på korken.">
				>
