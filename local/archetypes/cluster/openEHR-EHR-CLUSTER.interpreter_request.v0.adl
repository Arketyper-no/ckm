definition
	CLUSTER[at0000] matches {    -- Interpreter request
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0009] occurrences matches {0..1} matches {    -- Communication channel
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0010,    -- Face-to-face meeting
							at0011,    -- Videoconference
							at0012]    -- Audioconference
						}
					}
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0031] matches {    -- Language
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.language(-[a-zA-Z0-9_]+)*\.v0/}
			}
			ELEMENT[at0035] occurrences matches {0..1} matches {    -- Status
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0036,    -- Preferred language
							at0037]    -- Other language
						}
					}
				}
			}
			ELEMENT[at0033] occurrences matches {0..1} matches {    -- Professional required
				value matches {
					DV_BOOLEAN matches {
						value matches {true}
					}
				}
			}
			ELEMENT[at0027] occurrences matches {0..1} matches {    -- Preferred gender
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0028,    -- Male
							at0029,    -- Female
							at0034]    -- No preference
						}
					}
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Proposed interpreter
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.person_name(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.individual_professional(-[a-zA-Z0-9_]+)*\.v0/}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}



