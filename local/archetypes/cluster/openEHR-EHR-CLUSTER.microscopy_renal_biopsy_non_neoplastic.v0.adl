								list = <
									["1"] = <
										units = <"%">
										precision = <|0|>
									>
								>
							>
						}
					}
					ELEMENT[at0059] occurrences matches {0..1} matches {	-- Interstitium description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0060] occurrences matches {0..1} matches {	-- Vasculature
				items cardinality matches {1..*; unordered} matches {
