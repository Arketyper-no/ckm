archetype (adl_version=1.4)
	openEHR-EHR-CLUSTER.microscopy_renal_biopsy_non_neoplastic.v1

concept
	[at0000]	-- Microscopy renal biopsy non neoplastic
language
	original_language = <[ISO_639-1::nb]>
description
	original_author = <
		["name"] = <"Sabine Leh">
		["organisation"] = <"Haukeland University Hospital">
		["email"] = <"sabine.leh@helse-bergen.no">
		["date"] = <"2014-06-08">
	>
	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"To record detailed findings about a non-neoplastic renal biopsy">
			use = <"To record detailed findings about microscopic examination of tissue related to glomerular, tubulointerstitial and vascular renal diseases. Use as a component archetype in the context of a suite of archetypes that make up a histopathology report i.e. OBSERVATION.lab_test.histopathology">
			keywords = <"histopathology", "pathology", "renal", "laboratory">
			misuse = <"Not designed to be used within any other archetype other than OBSERVATION.lab_test.histopathology.">
			copyright = <"© Nasjonal IKT HF">
		>
	>
	lifecycle_state = <"Initial">
	other_contributors = <>
	other_details = <
		["current_contact"] = <"Sabine Leh, Haukeland University Hospital, sabine.leh@helse-bergen.no">
		["references"] = <"Chang A, Gibson IW, Cohen AH, Weening JW, Jennette JC, and Fogo AB (2012). A position paper on standardizing the nonneoplastic kidney biopsy report. Hum Pathol 43:1192-1196.
Farris AB, Adams CD, Brousaides N, Della Pelle PA, Collins AB, Moradi E, Smith RN, Grimm PC, and Colvin RB (2011). Morphometric and visual evaluation of fibrosis in renal biopsies. J Am Soc Nephrol 22:176-186.
Berden AE, Ferrario F, Hagen EC, Jayne DR, Jennette JC, Joh K, Neumann I, Noel LH, Pusey CD, Waldherr R, Bruijn JA, and Bajema IM (2010). Histopathologic classification of ANCA-associated glomerulonephritis. J Am Soc Nephrol 21:1628-1636.">
		["MD5-CAM-1.0.1"] = <"0D1286DB3D7DEE01DFAFCC167C535494">
	>

definition
