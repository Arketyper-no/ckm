ontology
	term_definitions = <
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Microscopy renal biopsy non neoplastic">
					description = <"Microscopic findings related to a non-neoplastic renal biopsy">
				>
				["at0001"] = <
					text = <"Renal biopsy overall description">
					description = <"*Describes tissue types  in the renal biopsy specimen">
				>
				["at0003"] = <
					text = <"Cortex">
					description = <"*Cortical tissue present, alternatively core length with cortical tissue, alternatively proportion of cortical tissue in relation to the core with renal tissue">
				>
				["at0004"] = <
					text = <"present">
					description = <"cortical tissue present">
				>
				["at0005"] = <
					text = <"absent">
					description = <"cortical tissue absent">
				>
				["at0006"] = <
					text = <"indeterminate">
					description = <"Presence of cortical tissue has not been determined">
				>
				["at0007"] = <
					text = <"Medulla">
					description = <"*Medullary tissue present, alternatively core length with medullary tissue, alternatively proportion of medullary tissue in relation to the core with renal tissue">
				>
				["at0008"] = <
					text = <"present">
					description = <"medullary tissue present">
				>
				["at0009"] = <
					text = <"absent">
