					text = <"renal pelvis mucosa">
					description = <"renal pelvis mucosa present">
				>
				["at0015"] = <
					text = <"Glomeruli">
					description = <"*Description of glomerular changes">
				>
				["at0016"] = <
					text = <"Glomeruli">
					description = <"*The maximum number of glomeruli  irrespective of size present in one section; structures consisting of Bowman's capsule with empty Bowman's space are excluded.">
				>
				["at0017"] = <
					text = <"Scorable glomeruli">
					description = <"*The number of glomeruli judged as adequate for scoring by the reporting pathologist">
				>
				["at0019"] = <
					text = <"Glomeruli solely with necrosis">
					description = <"*The number of glomeruli showing fibrinoid necrosis without crescent formation.">
				>
				["at0020"] = <
					text = <"Glomeruli with crescents">
					description = <"*The number of glomeruli showing cellular or fibrocellular crescents.">
				>
				["at0021"] = <
					text = <"Glomeruli with ischemic changes">
					description = <"*The number of glomeruli showing some degree of tuft retraction, wrinkling of capillary walls and fibrosis of Bowman's capsule">
				>
				["at0022"] = <
					text = <"Glomeruli with segmental sclerosis">
					description = <"*The number of glomeruli showing obliteration of capillary lumen by extracellular matrix in less than 50% of the tuft area with or without adhesions to the Bowman capsule, hyaline lesions, foam cells.">
				>
				["at0023"] = <
					text = <"Glomeruli with global sclerosis">
					description = <"*The number of glomeruli showing scarring with obliteration of capillary lumen by extracellular matrix in the entire or nearly entire glomerular tuft ">
				>
				["at0024"] = <
					text = <"Normal glomeruli">
					description = <"*The number of glomeruli which appear normal without major changes.">
				>
				["at0025"] = <
					text = <"Glomerular size">
					description = <"*The size of the glomerulus measured as mean of maximal tuft diameter of all measurable glomeruli">
				>
