						list = <
							["1"] = <
								units = <"µm">
							>
							["2"] = <
								units = <"mm">
							>
							["3"] = <
								units = <"cm">
							>
							["4"] = <
								units = <"m">
							>
							["5"] = <
								units = <"ft">
							>
							["6"] = <
								units = <"in">
							>
							["7"] = <
								units = <"yd">
							>
						>
					>
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {	-- Depth
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"µm">
							>
							["2"] = <
								units = <"mm">
							>
							["3"] = <
								units = <"cm">
							>
							["4"] = <
								units = <"m">
							>
							["5"] = <
