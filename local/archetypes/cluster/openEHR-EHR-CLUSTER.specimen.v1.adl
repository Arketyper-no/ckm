									at0058, 	-- Uhell ved laboratoriet
									at0059]	-- Teknisk feil
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Tilstrekkelighet for testen
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0062, 	-- Tilfredsstillende
