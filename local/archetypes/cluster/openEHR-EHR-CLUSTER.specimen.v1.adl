	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record details of a laboratory specimen. Will often be used in different contexts e.g within an Instruction archetype to describe the specimen that has to be taken, or describing the specimen which accompanies the laboratory request. It may occur within an Action archetype e.g. describing specimens taken as part of a surgical procedure.

It will finally be used within a laboratory result archetype to describe the specimen being reported. ">
			use = <"Generally used within OBSERVATION. Laboratory and other laboratory related Instruction and Action archetypes">
			keywords = <"specimen", "laboratory", "sample", "collection">
			misuse = <"">
			copyright = <"© Nasjonal IKT HF">
		>
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"For å registrere detaljer om en laboratorieprøve. Vil ofte bli benyttet i ulike sammenhenger, f.eks innenfor en INSTRUCTION-arketype for å beskrive den prøven som skal tas, eller beskrive prøven som følger en laboratorierekvisisjon. Den kan brukes i en ACTION-arketype f.eks som beskriver at prøven er tatt som en del av en kirurgisk prosedyre. 



Til slutt brukes den innen en laboratorieresultat-arketype for å beskrive prøven som blir rapportert.



">
			use = <"Normalt brukt innen OBSERVATION.Laboratory og andre laboratorierelaterte INSTRUCTION- og ACTION-arketyper.">
			keywords = <"prøve", "vevsprøve", "laboratorie", "samling", "sample", "blodprøve">
			misuse = <"">
			copyright = <"© Nasjonal IKT HF">
		>
	>
	lifecycle_state = <"AuthorDraft">
	other_contributors = <"Sam Heard Ocean Informatics", "Heather Leslie Ocean Informatics", "Koray Atalag University of Auckland">
	other_details = <
		["MD5-CAM-1.0.1"] = <"61426327CAF49DFD62017C694DD849C7">
	>

definition
	CLUSTER[at0000] matches {	-- Prøve
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0029] occurrences matches {0..1} matches {	-- Prøvetype
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {	-- Prøvetakingsprosedyre
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {	-- Sted
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
			}
			allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {	-- Fysiske egenskaper
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.physical_properties(-[a-zA-Z0-9_]+)*\.v1/}
			}
			CLUSTER[at0073] occurrences matches {0..1} matches {	-- Nålebiopsi egenskaper
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0074] occurrences matches {0..1} matches {	-- Diameter biopsinål
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0075] occurrences matches {0..1} matches {	-- Biopsisylinder lengde
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0076] occurrences matches {0..1} matches {	-- Antal vevssylindre
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
				}
			}
			CLUSTER[at0050] occurrences matches {0..1} matches {	-- Prøvetaking og håndtering
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Mulig risiko/biologisk risiko
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Forutsetninger for prøvetaking
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0080] occurrences matches {0..1} matches {	-- Antall beholdere
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0079] occurrences matches {0..1} matches {	-- Prøvetakingsmetode detaljer
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Transportmedium
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0068] occurrences matches {0..*} matches {	-- Ytterliggere prøvebehandling
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.specimen_preparation(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0078] occurrences matches {0..1} matches {	-- Testmetode
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0077] occurrences matches {0..*} matches {	-- Prøvetakingsutstyr
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
				}
			}
			CLUSTER[at0046] occurrences matches {0..1} matches {	-- Håndtering og prosessering
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Dato og klokkeslett for prøvetakingen
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0067] occurrences matches {0..1} matches {	-- Prøvetakingssituasjon
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0034] occurrences matches {0..1} matches {	-- Dato og klokkeslett for prøvemottak
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {	-- Dato og klokkeslett for avsluttet prosessering
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			CLUSTER[at0039] occurrences matches {0..1} matches {	-- Prøvekvalitet
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0042] occurrences matches {0..*} matches {	-- Problem ved prøvemottak
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0052, 	-- Hemolysert
									at0053, 	-- Lipemisk
									at0054, 	-- Feil transportmedium
									at0055]	-- Utilstrekkelig prøve
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0044] occurrences matches {0..*} matches {	-- Problem ved håndtering på laboratoriet
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
