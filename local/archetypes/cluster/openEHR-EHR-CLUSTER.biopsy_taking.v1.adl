definition
	CLUSTER[at0000] matches {	-- Biopsitaking
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0013] matches {	-- Biopsitype
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0021] occurrences matches {0..1} matches {	-- Leie
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {	-- Lokalisering
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0009, 	-- Ultralyd
							at0010, 	-- CT
							at0011, 	-- Gjennomlysning
							at0016, 	-- Åpen biopsi
							at0017, 	-- Palpatorisk
							at0018, 	-- Mammografi
							at0019, 	-- Endoskopisk
							at0020]	-- Visuelt på kroppsoverflate
						}
					}
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0022] occurrences matches {0..*} matches {	-- Biopsifeltgruppe
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0027] occurrences matches {0..*} matches {	-- Retning for inngrepet
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0012] occurrences matches {0..*} matches {	-- Prøvedetaljer
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.specimen(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0001] occurrences matches {0..1} matches {	-- Antall biopsiforsøk
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- Biopsifelt
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0025] occurrences matches {0..1} matches {	-- Visuell kontroll
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0026] occurrences matches {0..1} matches {	-- Bedømmelse
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}


