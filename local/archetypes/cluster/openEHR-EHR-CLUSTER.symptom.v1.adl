definition
	CLUSTER[at0000] matches {	-- Symptom
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] matches {	-- Symptomnavn
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0035] occurrences matches {0..1} matches {	-- Ikke tilstede
				value matches {
					DV_BOOLEAN matches {
						value matches {True}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {	-- Beskrivelse
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0151] occurrences matches {0..*} matches {	-- Anatomisk lokalisering
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0147] occurrences matches {0..*} matches {	-- Detaljert anatomisk lokalisering
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.relative_location(-[a-zA-Z0-9_]+)*\.v1/}
				exclude
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0152] occurrences matches {0..1} matches {	-- Dato/tid for debut
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0028] occurrences matches {0..1} matches {	-- Varighet
				value matches {
					DV_DURATION matches {*}
				}
			}
			ELEMENT[at0021] occurrences matches {0..1} matches {	-- Alvorlighetsgrad
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0023, 	-- Mild
							at0024, 	-- Moderat
							at0025]	-- Alvorlig
						}
					}
				}
			}
			ELEMENT[at0026] occurrences matches {0..1} matches {	-- Visuell analog skala
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::380]>
						list = <
							["1"] = <
								units = <"1">
								magnitude = <|0.0..10.0|>
								precision = <|1|>
							>
						>
					>
				}
			}
			ELEMENT[at0155] occurrences matches {0..1} matches {	-- Funksjonell innvirkning
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0153] occurrences matches {0..*} matches {	-- Spesifikke detaljer
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.pain(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {	-- Variasjon
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0004, 	-- Konstant
							at0005, 	-- Intermitterende
							at0006]	-- Fluktuerende
						}
					}
				}
			}
			ELEMENT[at0037] occurrences matches {0..1} matches {	-- Forløpsbeskrivelse
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0161] occurrences matches {0..1} matches {	-- Dato/tid for opphør
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0057] occurrences matches {0..1} matches {	-- Beskrivelse av tidligere episoder
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0018] occurrences matches {0..*} matches {	-- Innvirkende faktorer
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0019] occurrences matches {0..1} matches {	-- Faktor
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Effekt
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0156, 	-- Ingen effekt
									at0157, 	-- Utløser
									at0158, 	-- Forverrer
									at0159, 	-- Lindrer
									at0162]	-- Fjerner
								}
							}
						}
					}
					ELEMENT[at0056] occurrences matches {0..1} matches {	-- Beskrivelse
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0031] occurrences matches {0..1} matches {	-- Antall tidligere episoder
				value matches {
					DV_COUNT matches {
						magnitude matches {|>=0|}
					}
				}
			}
			allow_archetype CLUSTER[at0146] occurrences matches {0..*} matches {	-- Tidligere episode
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.symptom\.v1/}
			}
			allow_archetype CLUSTER[at0063] occurrences matches {0..*} matches {	-- Tilknyttede symptomer
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.symptom(-[a-zA-Z0-9_]+)*\.v1/}
			}
			allow_archetype CLUSTER[at0154] occurrences matches {0..*} matches {	-- Tilknyttede hendelser
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.health_event(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0163] occurrences matches {0..1} matches {	-- Kommentar
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}


ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["ar-sy"] = <
			items = <
				["at0000"] = <
					text = <"*Symptom(en)">
					description = <"*A subjective observation of a physical or mental disturbance in the body of an individual.(en)">
				>
				["at0001"] = <
					text = <"*Symptom name(en)">
					description = <"*The symptom experienced by the subject of care.(en)">
					comment = <"*Symptom name should be coded with a terminology, where possible.(en)">
				>
				["at0002"] = <
					text = <"*Description(en)">
					description = <"*Narrative description of the symptom as perceived by the subject of care.(en)">
				>
				["at0003"] = <
					text = <"*Variation(en)">
					description = <"*The variation of the symptom during its course since onset.(en)">
				>
				["at0004"] = <
					text = <"ثابت">
					description = <"العرض موجود بشكل ثابت/مستمر">
				>
				["at0005"] = <
					text = <"متقطع">
					description = <"العرض موجود في بعض الأوقات و غائب في البعض الآخر">
				>
				["at0006"] = <
					text = <"متموج">
					description = <"العرض يتغير بشكل مؤثر و لكنه موجود في جميع الأوقات">
				>
				["at0017"] = <
					text = <"*Effect(en)">
					description = <"*Perceived outcome after exposure of the variable on the symptom.(en)">
				>
				["at0018"] = <
					text = <"*Modifying Factor(en)">
					description = <"*Detail about how a specific factor impacts the symptom.(en)">
				>
				["at0019"] = <
					text = <"*Factor(en)">
					description = <"*Name of an activity which might impact on the symptom.(en)">
					comment = <"*Examples of activities: lying on multiple pillows, eating or administration of medication.(en)">
				>
				["at0021"] = <
					text = <"*Severity(en)">
					description = <"*The overall intensity of the symptom as reported by the individual.(en)">
					comment = <"*Defining values such as mild, moderate or severe in such a way that is applicable to multiple symptoms plus allows multiple users to interpret and record them consistently is not easy. To extend the value set further with inclusion of additional values such as 'Trivial' and 'Very severe', and/or 'Mild-Moderate' and 'Moderate-Severe', adds to the definitional difficulty and may also worsen inter-recorder reliability issues. Use of 'Life-threatening' and 'Fatal' is also often considered as part of this value set, although from a pure point of view it may actually reflect an outcome rather than a severity. In view of the above, keeping to a well-defined but smaller list is preferred. Note: more specific grading of severity can be recorded using the 'Specific details' SLOT.(en)">
				>
				["at0023"] = <
					text = <"*Mild(en)">
					description = <"*The intensity of the symptom does not cause interference with normal activity.(en)">
				>
				["at0024"] = <
					text = <"*Moderate(en)">
					description = <"*The intensity of the symptom causes interference with normal activity.(en)">
				>
				["at0025"] = <
					text = <"*Severe(en)">
					description = <"*The intensity of the symptom causes prevents normal activity.(en)">
				>
				["at0026"] = <
					text = <"*Visual analogue score(en)">
					description = <"*Rating scale for severity of the symptom.(en)">
					comment = <"*Symptom severity can be rated by the individual by recording a score from 0 (symptom not present) to 10 (symptom is as severe as the individual can imagine).(en)">
				>
				["at0028"] = <
					text = <"*Duration(en)">
					description = <"*The duration of the symptom since onset.(en)">
					comment = <"*If 'Date/time of onset' and 'Date/time of resolution' are used in systems, this data element may be calculated, or alternatively, considered redundant.(en)">
				>
				["at0031"] = <
					text = <"*Number of previous episodes(en)">
					description = <"*The number of times this symptom has previously occurred.(en)">
				>
				["at0035"] = <
					text = <"*Not present(en)">
					description = <"*The individual has not experienced the symptom.(en)">
					comment = <"*Record as True if the subject of care has not experienced the symptom.(en)">
				>
				["at0037"] = <
					text = <"*Course description(en)">
					description = <"*Narrative description about the course of the symptom since onset.(en)">
					comment = <"*For example: a text description of the immediate onset of the symptom, activities that worsened or relieved the symptom, whether it is improving or worsening and how it resolved over weeks.(en)">
				>
				["at0056"] = <
					text = <"*Description(en)">
					description = <"*Narrative description of the effect of the modifying factor on the symptom.(en)">
				>
				["at0057"] = <
					text = <"*Description of previous episodes(en)">
					description = <"*Narrative description of any or all previous episodes.(en)">
					comment = <"*  May include a comparison to this episode.(en)">
				>
				["at0063"] = <
					text = <"*Associated symptoms(en)">
					description = <"*Structured details about any associated symptoms that are concurrent.(en)">
				>
				["at0146"] = <
					text = <"*Previous episode(en)">
					description = <"*Structured details of the symptom during a previous episode.(en)">
					comment = <"*In linked clinical systems, it is possible that previous epsiodes are already recorded within the EHR. Systems can allow the clinician to link to relevant previous episodes.
In an unlinked system or with a new patient, no previous episodes will be available and additional instances of the symptom archetype could be used to record the previous episodes. (en)">
				>
				["at0147"] = <
					text = <"*Detailed anatomical location(en)">
					description = <"*Slot to contain detailed and structured anatomical location details.(en)">
					comment = <"*If the anatomical location is included in the Symptom name via precoordinated codes, use of this SLOT becomes redundant.(en)">
				>
				["at0151"] = <
					text = <"*Anatomical location(en)">
					description = <"*Record of a simple site on the body where the symptom was located.(en)">
					comment = <"*Occurrences for this data element are unbounded to allow for clinical scenarios such as describing a pain in multiple locations but where all of the other attributes such as impact and duration are identical. If the requirements for recording the body site are determined at run-time by the application or require more complex modelling such as relative locations then use the CLUSTER.anatomical_location or CLUSTER.relative_location within the Detailed anatomical location' SLOT in this archetype. 
If the anatomical location is included in the Symptom name via precoordinated codes, this data element becomes redundant.(en)">
				>
				["at0152"] = <
					text = <"*Date/time of onset(en)">
					description = <"*The timing of the onset for this episode of the symptom.(en)">
					comment = <"*While partial dates are permitted, the exact date and time of onset can be recorded, if appropriate.(en)">
				>
				["at0153"] = <
					text = <"*Specific details(en)">
					description = <"*Specific data elements that are additionally required to record as unique attributes of the identified symptom.(en)">
					comment = <"*For example: description of pain can require additional data elements describing the radiation and visual analogue rating scales; description of sore throat may include presence of fever or rigors, and difficulty swallowing.(en)">
				>
				["at0154"] = <
					text = <"*Associated events(en)">
					description = <"*Structured details about any associated symptoms that are concurrent.(en)">
				>
				["at0155"] = <
					text = <"*Impact(en)">
					description = <"*Description of the functional impact of this symptom.(en)">
					comment = <"*Assessment of impact should consider the severity, duration and frequency of the symptom. Examples for hearing loss may include: 'Difficulty Hearing in Quiet Environment'; 'Difficulty Hearing the TV or Radio'; 'Difficulty Hearing Group Conversation'; and 'Difficulty Hearing on Phone'.(en)">
				>
				["at0156"] = <
					text = <"*No effect(en)">
					description = <"*Presence of the factor has no impact on the symptom.(en)">
				>
				["at0157"] = <
					text = <"*Precipitates(en)">
					description = <"*Presence of the factor causes onset of the symptom.(en)">
				>
				["at0158"] = <
					text = <"*Worsens(en)">
					description = <"*Presence of the factor exaccerbates the severity or impact of the symptom.(en)">
				>
				["at0159"] = <
					text = <"*Relieves(en)">
					description = <"*Presence of the factor reduces the severity or impact of the symptom.(en)">
				>
				["at0161"] = <
					text = <"*Date/time of resolution(en)">
					description = <"*The timing of the cessation of this episode of the symptom.(en)">
					comment = <"*If 'Date/time of onset' and 'Duration' are used in systems, this data element may be calculated, or alternatively, considered redundant. While partial dates are permitted, the exact date and time of resolution can be recorded, if appropriate.(en)">
				>
				["at0162"] = <
					text = <"*Resolves(en)">
					description = <"*Presence of the factor resolves the symptom.(en)">
				>
				["at0163"] = <
					text = <"*Comment(en)">
					description = <"*Additional narrative about the symptom not captured in other fields.(en)">
				>
			>
		>
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Symptom">
					description = <"A subjective observation of a physical or mental disturbance in the body of an individual.">
				>
				["at0001"] = <
					text = <"Symptom name">
					description = <"The symptom experienced by the subject of care.">
					comment = <"Symptom name should be coded with a terminology, where possible.">
				>
				["at0002"] = <
					text = <"Description">
					description = <"Narrative description of the symptom as perceived by the subject of care.">
				>
				["at0003"] = <
					text = <"Variation">
					description = <"The variation of the symptom during its course since onset.">
				>
				["at0004"] = <
					text = <"Constant">
					description = <"The symptom is constantly present.">
				>
				["at0005"] = <
					text = <"Intermittent">
					description = <"The symptom is present at times and not at others.">
				>
				["at0006"] = <
					text = <"Fluctuating">
					description = <"The symptom varys considerably but is almost always present.">
				>
				["at0017"] = <
					text = <"Effect">
					description = <"Perceived outcome after exposure of the variable on the symptom.">
				>
				["at0018"] = <
					text = <"Modifying Factor">
					description = <"Detail about how a specific factor impacts the symptom.">
				>
				["at0019"] = <
					text = <"Factor">
					description = <"Name of an activity which might impact on the symptom.">
					comment = <"Examples of activities: lying on multiple pillows, eating or administration of medication.">
				>
				["at0021"] = <
					text = <"Severity">
					description = <"The overall intensity of the symptom as reported by the individual.">
					comment = <"Defining values such as mild, moderate or severe in such a way that is applicable to multiple symptoms plus allows multiple users to interpret and record them consistently is not easy. To extend the value set further with inclusion of additional values such as 'Trivial' and 'Very severe', and/or 'Mild-Moderate' and 'Moderate-Severe', adds to the definitional difficulty and may also worsen inter-recorder reliability issues. Use of 'Life-threatening' and 'Fatal' is also often considered as part of this value set, although from a pure point of view it may actually reflect an outcome rather than a severity. In view of the above, keeping to a well-defined but smaller list is preferred. Note: more specific grading of severity can be recorded using the 'Specific details' SLOT.">
				>
				["at0023"] = <
					text = <"Mild">
					description = <"The intensity of the symptom does not cause interference with normal activity.">
				>
				["at0024"] = <
					text = <"Moderate">
					description = <"The intensity of the symptom causes interference with normal activity.">
				>
				["at0025"] = <
					text = <"Severe">
					description = <"The intensity of the symptom causes prevents normal activity.">
				>
				["at0026"] = <
					text = <"Visual analogue score">
					description = <"Rating scale for severity of the symptom.">
					comment = <"Symptom severity can be rated by the individual by recording a score from 0 (symptom not present) to 10 (symptom is as severe as the individual can imagine).">
				>
				["at0028"] = <
					text = <"Duration">
					description = <"The duration of the symptom since onset.">
					comment = <"If 'Date/time of onset' and 'Date/time of resolution' are used in systems, this data element may be calculated, or alternatively, considered redundant.">
				>
				["at0031"] = <
					text = <"Number of previous episodes">
					description = <"The number of times this symptom has previously occurred.">
				>
				["at0035"] = <
					text = <"Not present">
					description = <"The individual has not experienced the symptom.">
					comment = <"Record as True if the subject of care has not experienced the symptom.">
				>
				["at0037"] = <
					text = <"Course description">
					description = <"Narrative description about the course of the symptom since onset.">
					comment = <"For example: a text description of the immediate onset of the symptom, activities that worsened or relieved the symptom, whether it is improving or worsening and how it resolved over weeks.">
				>
				["at0056"] = <
					text = <"Description">
					description = <"Narrative description of the effect of the modifying factor on the symptom.">
				>
				["at0057"] = <
					text = <"Description of previous episodes">
					description = <"Narrative description of any or all previous episodes.">
					comment = <"  May include a comparison to this episode.">
				>
				["at0063"] = <
					text = <"Associated symptoms">
					description = <"Structured details about any associated symptoms that are concurrent.">
				>
				["at0146"] = <
					text = <"Previous episode">
					description = <"Structured details of the symptom during a previous episode.">
					comment = <"In linked clinical systems, it is possible that previous epsiodes are already recorded within the EHR. Systems can allow the clinician to link to relevant previous episodes.
In an unlinked system or with a new patient, no previous episodes will be available and additional instances of the symptom archetype could be used to record the previous episodes. ">
				>
				["at0147"] = <
					text = <"Detailed anatomical location">
					description = <"Slot to contain detailed and structured anatomical location details.">
					comment = <"If the anatomical location is included in the Symptom name via precoordinated codes, use of this SLOT becomes redundant.">
				>
				["at0151"] = <
					text = <"Anatomical location">
					description = <"Record of a simple site on the body where the symptom was located.">
					comment = <"Occurrences for this data element are unbounded to allow for clinical scenarios such as describing a pain in multiple locations but where all of the other attributes such as impact and duration are identical. If the requirements for recording the body site are determined at run-time by the application or require more complex modelling such as relative locations then use the CLUSTER.anatomical_location or CLUSTER.relative_location within the Detailed anatomical location' SLOT in this archetype. 
If the anatomical location is included in the Symptom name via precoordinated codes, this data element becomes redundant.">
				>
				["at0152"] = <
					text = <"Date/time of onset">
					description = <"The timing of the onset for this episode of the symptom.">
					comment = <"While partial dates are permitted, the exact date and time of onset can be recorded, if appropriate.">
				>
				["at0153"] = <
					text = <"Specific details">
					description = <"Specific data elements that are additionally required to record as unique attributes of the identified symptom.">
					comment = <"For example: description of pain can require additional data elements describing the radiation and visual analogue rating scales; description of sore throat may include presence of fever or rigors, and difficulty swallowing.">
				>
				["at0154"] = <
					text = <"Associated events">
					description = <"Structured details about any associated symptoms that are concurrent.">
				>
				["at0155"] = <
					text = <"Impact">
					description = <"Description of the functional impact of this symptom.">
					comment = <"Assessment of impact should consider the severity, duration and frequency of the symptom. Examples for hearing loss may include: 'Difficulty Hearing in Quiet Environment'; 'Difficulty Hearing the TV or Radio'; 'Difficulty Hearing Group Conversation'; and 'Difficulty Hearing on Phone'.">
				>
				["at0156"] = <
					text = <"No effect">
					description = <"Presence of the factor has no impact on the symptom.">
				>
				["at0157"] = <
					text = <"Precipitates">
					description = <"Presence of the factor causes onset of the symptom.">
				>
				["at0158"] = <
					text = <"Worsens">
					description = <"Presence of the factor exaccerbates the severity or impact of the symptom.">
				>
				["at0159"] = <
					text = <"Relieves">
					description = <"Presence of the factor reduces the severity or impact of the symptom.">
				>
				["at0161"] = <
					text = <"Date/time of resolution">
					description = <"The timing of the cessation of this episode of the symptom.">
					comment = <"If 'Date/time of onset' and 'Duration' are used in systems, this data element may be calculated, or alternatively, considered redundant. While partial dates are permitted, the exact date and time of resolution can be recorded, if appropriate.">
				>
				["at0162"] = <
					text = <"Resolves">
					description = <"Presence of the factor resolves the symptom.">
				>
				["at0163"] = <
					text = <"Comment">
					description = <"Additional narrative about the symptom not captured in other fields.">
				>
			>
		>
