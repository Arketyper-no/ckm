definition
	OBSERVATION[at0000] matches {    -- Chelsea Critical Care Physical Assessment Tool (CPAx)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Respiratory function
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009],
											5|[local::at0010]
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Cough
										value matches {
											0|[local::at0021],
											1|[local::at0022],
											2|[local::at0023],
											3|[local::at0024],
											4|[local::at0025],
											5|[local::at0026]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Moving within the bed (e.g. rolling)
										value matches {
											0|[local::at0027],
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030],
											4|[local::at0031],
											5|[local::at0032]
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- Supine to sitting on the edge of the bed
										value matches {
											0|[local::at0033],
											1|[local::at0034],
											2|[local::at0035],
											3|[local::at0036],
											4|[local::at0037],
											5|[local::at0038]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Dynamic sitting (i.e. when sitting on the edge of the bed/unsupported sitting)
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043],
											5|[local::at0044]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Standing balance
										value matches {
											0|[local::at0045],
											1|[local::at0046],
											2|[local::at0047],
											3|[local::at0048],
											4|[local::at0049],
											5|[local::at0050]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Sit to stand (starting position: ≤90º hip flexion)
										value matches {
											0|[local::at0051],
											1|[local::at0052],
											2|[local::at0053],
											3|[local::at0054],
											4|[local::at0055],
											5|[local::at0056]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Transferring from bed to chair
										value matches {
											0|[local::at0057],
											1|[local::at0058],
											2|[local::at0059],
											3|[local::at0060],
											4|[local::at0061],
											5|[local::at0062]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Stepping
										value matches {
											0|[local::at0063],
											1|[local::at0064],
											2|[local::at0065],
											3|[local::at0066],
											4|[local::at0067],
											5|[local::at0068]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Grip strength (predicted mean for age and gender on the strongest hand)
										value matches {
											0|[local::at0069],
											1|[local::at0070],
											2|[local::at0071],
											3|[local::at0072],
											4|[local::at0073],
											5|[local::at0074]
										}
									}
									ELEMENT[at0076] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..50|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}



