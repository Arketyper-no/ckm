			items = <
				["at0000"] = <
					text = <"Spørreskjema om sykdomshåndtering">
					description = <"Svar på forhåndsdefinerte spørsmål om håndtering av medisinske tilstander, inkludert behandling.">
					comment = <"Svarene kan være selvrapporterte.">
				>
				["at0001"] = <
					text = <"History">
