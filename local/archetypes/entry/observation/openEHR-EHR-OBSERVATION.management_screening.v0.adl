					description = <"Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i et templat eller i en applikasjon.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Navn på sykdomshåndtering/behandling">
					description = <"Navn på en spesifikk sykdomshåndterings- eller behandlingsaktivitet eller gruppering av sykdomshåndterings- eller behandlingsaktiviteter.">
					comment = <"Koding av \"Navn på sykdomshåndtering/behandling\" med en terminologi foretrekkes, der det er mulig. For eksempel: Sykehusinnleggelse, innlagt på intensivavdeling.">
				>
				["at0005"] = <
					text = <"Er den spesifikke sykdomshåndteringen eller behandlingen utført?">
					description = <"Tilstedeværelse av en spesifikk sykdomshåndterings- eller behandlingsaktivitet relatert til kartleggingsformålet.">
				>
				["at0007"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0021"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0022"] = <
					text = <"Sykdomshåndterings- eller behandlingsaktivitet">
					description = <"Detaljer om en spesifikk sykdomshåndterings- eller behandlingsaktivitet eller gruppering av sykdomshåndterings- eller behandlingsaktiviteter relatert til kartleggingsformålet.">
				>
				["at0023"] = <
					text = <"Ja">
					description = <"Den spesifikke aktiviteten er tilstede.">
				>
				["at0024"] = <
					text = <"Nei">
					description = <"Den spesifikke aktiviteten er fraværende.">
				>
				["at0027"] = <
					text = <"Usikker">
					description = <"Det er ukjent om den spesifikke aktiviteten er tilstede eller fraværende.">
				>
				["at0034"] = <
					text = <"Kartleggingsformål">
					description = <"Konteksten eller årsaken for kartleggingen.">
					comment = <"Dette dataelementet er ment for å sette en kontekst for spørsmålene om man senere ønsker å gjøre spørringer på dataene. Det er ikke forventet at dette dataelementet skal være synlig for en bruker av skjemaet, men kun tilgjengelig i de lagrede dataene. For eksempel: \"Pre operativ screening\" eller navnet på det faktiske spørreskjemaet.">
				>
				["at0035"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om det spesifikke sykdomshåndteringen eller behandlingen eller gruppen av sykdomshåndterings- eller behandlingsaktiviteter som ikke er omfattet av andre felt.">
				>
				["at0036"] = <
					text = <"Ytterligere detaljer">
					description = <"Strukturerte detaljer eller spørsmål om den spesifikke sykdomshåndteringen eller behandlingen.">
					comment = <"For eksempel: Behandlingsinstitusjon.">
				>
				["at0037"] = <
					text = <"Start">
					description = <"Når sykdomshåndteringen eller behandlingen startet.">
