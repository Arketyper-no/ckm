	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Disse spørsmålene er utarbeidet for å gi oss informasjon om hvordan dine smerter har påvirket dine muligheter til å klare dagliglivet ditt. Vær snill å besvare spørsmålene ved å sette kryss (kun ett kryss for hvert avsnitt) i de rutene som passer best for deg.">
			use = <"Ett kryss i hvert avsnitt.">
			keywords = <"Oswestry", ...>
			misuse = <"Ingen eller mer enn ett kryss per avsnitt.">
			copyright = <"© Nasjonal IKT HF">
		>
	>
