definition
	OBSERVATION[at0000] matches {	-- Oswestry
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {10..*; ordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Smerte
										value matches {
											0|[local::at0007], 	-- Jeg har ingen smerter for øyeblikket
											1|[local::at0008], 	-- Smertene er veldig svake for øyeblikket
											2|[local::at0009], 	-- Smertene er moderate for øyeblikket
											3|[local::at0010], 	-- Smertene er temmelig sterke for øyeblikket
											4|[local::at0011], 	-- Smertene er veldig sterke for øyeblikket
											5|[local::at0012]  	-- Smertene er det verste jeg kan tenke meg for øyeblikket
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Personlig stell
										value matches {
											0|[local::at0013], 	-- Jeg kan stelle meg selv på vanlig måte uten at det forårsaker ekstra smerter
											1|[local::at0014], 	-- Jeg kan stelle meg selv på vanlig måte, men det er veldig smertefullt
											2|[local::at0015], 	-- Det er smertefullt å stelle seg selv, og jeg gjør det langsomt og forsiktig
											3|[local::at0016], 	-- Jeg trenger noe hjelp, men klarer det meste av mitt personlige stell
											4|[local::at0017], 	-- Jeg trenger hjelp hver dag til det meste av eget stell
											5|[local::at0018]  	-- Jeg kler ikke på meg, har vanskeligheter med å vaske meg og holder sengen
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {	-- Å løfte
										value matches {
											0|[local::at0020], 	-- Jeg kan løfte tunge ting uten å få mer smerter
											1|[local::at0021], 	-- Jeg kan løfte tunge ting, men får mer smerter
											2|[local::at0022], 	-- Smertene hindrer meg i å løfte tunge ting opp fra gulvet, men jeg greier det hvis det som skal løftes er gunstig plassert, for eksempel på et bord
											3|[local::at0023], 	-- Smertene hindrer meg i å løfte tunge ting, men jeg klarer lette og middels tunge ting, hvis det er gunstig plassert
											4|[local::at0024], 	-- Jeg kan bare løfte noe som er veldig lett
											5|[local::at0025]  	-- Jeg kan ikke løfte eller bære noe i det hele tatt
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {	-- Å gå
										value matches {
											0|[local::at0027], 	-- Smerter hindrer meg ikke i å gå i det hele tatt
											1|[local::at0028], 	-- Smerter hindrer meg i å gå mer enn 1,5 km
											2|[local::at0029], 	-- Smerter hindrer meg i å gå mer enn 3/4 km
											3|[local::at0030], 	-- Smerter hindrer meg i å gå mer enn 100 m
											4|[local::at0031], 	-- Jeg kan bare gå med stokk eller krykker
											5|[local::at0032]  	-- Jeg ligger for det meste i sengen, og jeg må krabbe til toalettet
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {	-- Å sitte
										value matches {
											0|[local::at0034], 	-- Jeg kan sitte så lenge jeg vil i en hvilken som helst stol
											1|[local::at0035], 	-- Jeg kan sitte så lenge jeg vil i min favorittstol
											2|[local::at0036], 	-- Smerter hindrer meg i å sitte mer enn en time
											3|[local::at0037], 	-- Smerter hindrer meg i å sitte mer enn en halv time
											4|[local::at0038], 	-- Smerter hindrer meg i å sitte mer enn ti minutter
											5|[local::at0039]  	-- Smerter hindrer meg i å sitte i det hele tatt
										}
									}
									ELEMENT[at0068] occurrences matches {0..1} matches {	-- Å stå
										value matches {
											0|[local::at0069], 	-- Jeg kan stå så lenge jeg vil uten å få smerter
											1|[local::at0070], 	-- Jeg kan stå så lenge jeg vil, men får mer smerter
											2|[local::at0071], 	-- Smerter hindrer meg i å stå mer enn en time
											3|[local::at0072], 	-- Smerter hindrer meg i å stå mer enn en halv time
											4|[local::at0073], 	-- Smerter hindrer meg i å stå mer enn ti minutter
											5|[local::at0074]  	-- Smerter hindrer meg i å stå i det hele tatt
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {	-- Å sove
										value matches {
											0|[local::at0041], 	-- Søvnen min forstyrres aldri av smerter
											1|[local::at0042], 	-- Søvnen min forstyrres av og til av smerter
											2|[local::at0043], 	-- På grunn av smerter får jeg mindre enn seks timers søvn
											3|[local::at0044], 	-- På grunn av smerter får jeg mindre enn fire timers søvn
											4|[local::at0045], 	-- På grunn av smerter får jeg mindre enn to timers søvn
											5|[local::at0046]  	-- Smerter hindrer all søvn
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {	-- Seksualliv
										value matches {
											0|[local::at0048], 	-- Seksuallivet mitt er normalt og forårsaker ikke mer smerter
											1|[local::at0049], 	-- Seksuallivet mitt er normalt, men forårsaker noe mer smerter
											2|[local::at0050], 	-- Seksuallivet mitt er normalt, men svært smertefullt
											3|[local::at0051], 	-- Seksuallivet mitt er svært begrenset av smerter
											4|[local::at0052], 	-- Seksuallivet mitt er nesten borte på grunn av smerter
											5|[local::at0053]  	-- Smerter forhindrer alt seksualliv
										}
									}
									ELEMENT[at0054] occurrences matches {0..1} matches {	-- Sosialt liv (omgang med venner og kjente)
										value matches {
											0|[local::at0055], 	-- Det sosiale livet mitt er normalt og forårsaker ikke mer smerter
											1|[local::at0056], 	-- Det sosiale livet mitt er normalt, men øker graden av smerter
											2|[local::at0057], 	-- Smerter har ingen betydelig innvirkning på mitt sosiale liv, bortsett fra at de begrenser mine mer fysisk aktive sider, som sport osv.
											3|[local::at0058], 	-- Smerter har begrenset mitt sosiale liv, og jeg går ikke så ofte ut
											4|[local::at0059], 	-- Smerter har begrenset mitt sosiale liv til hjemmet
											5|[local::at0060]  	-- På grunn av smerter har jeg ikke noe sosialt liv
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {	-- Å reise
										value matches {
											0|[local::at0062], 	-- Jeg kan reise hvor som helst uten smerter
											1|[local::at0063], 	-- Jeg kan reise hvor som helst, men det gir mer smerter
											2|[local::at0064], 	-- Smertene er ille, men jeg klarer reiser på to timer
											3|[local::at0065], 	-- Smerter begrenser meg til korte reiser på under en time
											4|[local::at0066], 	-- Smerter begrenser meg til korte, nødvendige reiser på under 30 minutter
											5|[local::at0067]  	-- Smerter forhindrer meg fra å reise, unntatt for å få behandling
										}
									}
									ELEMENT[at0075] occurrences matches {0..1} matches {	-- Oswestry score
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"%">
														magnitude = <|0.0..100.0|>
													>
												>
											>
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


