definition
	OBSERVATION[at0000] matches {    -- Gugging Swallowing Screen for Intensive Care Units (GUSS - ICU)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {    -- Preliminary Investigation / Indirect Swallowing Test
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {    -- RASS from 0 to +2
												value matches {
													1|[local::at0006],
													0|[local::at0007]
												}
											}
											ELEMENT[at0035] occurrences matches {0..1} matches {    -- Stridor present
												value matches {
													0|[local::at0036],
													1|[local::at0037]
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {    -- Coughing and/or throat clearing efficiently
												value matches {
													1|[local::at0012],
													0|[local::at0013]
												}
											}
											ELEMENT[at0009] occurrences matches {0..1} matches {    -- Swallowing saliva possible
												value matches {
													1|[local::at0014],
													0|[local::at0015]
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- Drooling (saliva)
												value matches {
													0|[local::at0016],
													1|[local::at0017]
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {    -- Change of voice after swallowing saliva
												value matches {
													0|[local::at0018],
													1|[local::at0019]
												}
											}
											ELEMENT[at0020] occurrences matches {0..1} matches {    -- Sum
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..6|}
													}
												}
											}
										}
									}
									CLUSTER[at0021] occurrences matches {0..1} matches {    -- Direct Swallowing Test (4 subtests)
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0022] occurrences matches {0..1} matches {    -- Semisolid
												value matches {
													1|[local::at0039],
													0|[local::at0040]
												}
											}
											ELEMENT[at0023] occurrences matches {0..1} matches {    -- Liquids
												value matches {
													1|[local::at0047],
													0|[local::at0048]
												}
											}
											ELEMENT[at0024] occurrences matches {0..1} matches {    -- Solids
												value matches {
													1|[local::at0049],
													0|[local::at0050]
												}
											}
											ELEMENT[at0025] occurrences matches {0..1} matches {    -- Liquids & Solids
												value matches {
													1|[local::at0051],
													0|[local::at0052]
												}
											}
											ELEMENT[at0026] occurrences matches {0..1} matches {    -- Sum
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..4|}
													}
												}
											}
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- Total sum
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..10|}
											}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {    -- Severity code
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0031,    -- Preliminary investigation or semisolids failed. (Severe Dysphagia with high risk of aspiration)
													at0032,    -- Semisolids passed, fluids failed. (Moderate dysphagia with aspiration risk)
													at0033,    -- Semisolids passed, fluids passed,solids failed. (Mild dysphagia with low risk of aspiration)
													at0034,    -- Semisolids passed, fluids passed, solids passed, mixed textures failed. (Mild dysphagia with low risk of aspiration)
													at0038]    -- All textures passed. (Minimal/no Dysphagia; Minimal/no risk of aspiration)
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0028] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



