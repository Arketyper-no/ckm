				["at0034"] = <
					text = <"Luft eller syrgas?">
					description = <"*Assessment carried out breathing air or supplemental oxygen.(en)">
				>
				["at0036"] = <
					text = <"*Air(en)">
					description = <"*The patient is not receiving supplemental oxygen; scored as 0 points.(en)">
				>
				["at0037"] = <
					text = <"*Oxygen(en)">
					description = <"*The patient is receiving supplemental oxygen; scored as 2 points.(en)">
				>
				["at0038"] = <
					text = <"*>=39.1(en)">
					description = <"*The body temperature is greater than/equal to 39.1 degrees Celcius; scored as 2 points.(en)">
				>
				["at0039"] = <
					text = <"*<=35.0(en)">
					description = <"*The body temperature is less than/equal to 35 degrees Celcius ; scored as 3 points.(en)">
