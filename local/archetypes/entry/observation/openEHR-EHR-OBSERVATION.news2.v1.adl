definition
	OBSERVATION[at0000] matches {    -- NEWS2 Score
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Any point in time event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Respiration rate
										value matches {
											3|[local::at0021],
											1|[local::at0019],
											0|[local::at0018],
											2|[local::at0020],
											3|[local::at0064]
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {    -- SpO₂ Scale 1
										value matches {
											3|[local::at0033],
											2|[local::at0032],
											1|[local::at0031],
											0|[local::at0030]
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {    -- SpO₂ Scale 2
										value matches {
											3|[local::at0061],
											2|[local::at0050],
											1|[local::at0049],
											0|[local::at0048],
											1|[local::at0062],
											2|[local::at0063],
											3|[local::at0051]
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Air or oxygen?
										value matches {
											0|[local::at0036],
											2|[local::at0037]
										}
									}
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Systolic blood pressure
										value matches {
											3|[local::at0017],
											2|[local::at0016],
											1|[local::at0015],
											0|[local::at0014],
											3|[local::at0067]
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Pulse
										value matches {
											3|[local::at0010],
											1|[local::at0012],
											0|[local::at0013],
											1|[local::at0068],
											2|[local::at0011],
											3|[local::at0065]
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Consciousness
										value matches {
											0|[local::at0024],
											3|[local::at0025]
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Temperature
										value matches {
											3|[local::at0039],
											1|[local::at0023],
											0|[local::at0022],
											1|[local::at0066],
											2|[local::at0038]
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..20|}
											}
										}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Clinical risk category
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0057,    -- Low
													at0058,    -- Low-medium
													at0059,    -- Medium
													at0060]    -- High
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0045] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"NEWS2 Score">
					description = <"A simple assessment score used to identify clinical deterioration in a patient.">
					comment = <"This is the second version of the National Early Warning Score (NEWS), issued by the UK Royal College of Physicians in 2017.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any point in time event">
					description = <"Default, unspecified point in time event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Systolic blood pressure">
					description = <"Category for the systolic blood pressure measurement.">
				>
				["at0005"] = <
					text = <"Pulse">
					description = <"Category for the pulse measurement.">
				>
				["at0006"] = <
					text = <"Respiration rate">
					description = <"Category for the respiratory rate measurement.">
				>
				["at0007"] = <
					text = <"Temperature">
					description = <"Range category for the body temperature measurement.">
				>
				["at0008"] = <
					text = <"Consciousness">
					description = <"Category for the observed conscious state, using the ACVPU scale.">
				>
				["at0010"] = <
					text = <"≤40">
					description = <"The pulse measurement is less than/equal to 40 beats/min.">
				>
				["at0011"] = <
					text = <"111-130">
					description = <"The pulse measurement is between 111 and 130 beats/min.">
				>
				["at0012"] = <
					text = <"41-50">
					description = <"The pulse measurement is between 41 and 50 beats/min.">
				>
				["at0013"] = <
					text = <"51-90">
					description = <"The pulse measurement is between 51 and 90 beats/min.">
				>
				["at0014"] = <
					text = <"111-219">
					description = <"The systolic blood pressure measurement is between 111 and 219 mmHg.">
				>
				["at0015"] = <
					text = <"101-110">
					description = <"The systolic blood pressure measurement is between 101 and 110 mmHg.">
				>
				["at0016"] = <
					text = <"91-100">
					description = <"The systolic blood pressure measurement is between 91 and 100 mmHg.">
				>
				["at0017"] = <
					text = <"≤90">
					description = <"The systolic blood pressure measurement is less than/equal to 90 mmHg.">
				>
				["at0018"] = <
					text = <"12-20">
					description = <"The respiratory rate measurement is between 12 and 20 breaths/min.">
				>
				["at0019"] = <
					text = <"9-11">
					description = <"The respiratory rate measurement is between 9 and 11 breaths/min.">
				>
				["at0020"] = <
					text = <"21-24">
					description = <"The respiratory rate measurement is between 21 and 24 breaths/min.">
				>
				["at0021"] = <
					text = <"≤8">
					description = <"The respiratory rate measurement is less than/equal to 8 breaths/min.">
				>
				["at0022"] = <
					text = <"36.1-38.0">
					description = <"The body temperature measurement is between 36.1 and 38.0 degrees Celsius.">
				>
				["at0023"] = <
					text = <"35.1-36.0">
					description = <"The body temperature measurement is between 35.1 and 36.0 degrees Celsius.">
				>
				["at0024"] = <
					text = <"Alert">
					description = <"The patient is alert or awake; scored as 0 points.">
				>
				["at0025"] = <
					text = <"C, V, P or U">
					description = <"The patient is newly confused, responds only to voice or pain, or is unresponsive; scored as 3 points.">
				>
				["at0028"] = <
					text = <"Total score">
					description = <"The sum of points assigned for each of the component variables.">
					comment = <"The total score may be generated at run-time.">
				>
				["at0029"] = <
					text = <"SpO₂ Scale 1">
					description = <"Category for the oxygen saturation measurement.">
					comment = <"'SpO₂ Scale 1' and 'SpO₂ Scale 2' are variations of the same physiological variable to be used in different clinical scenarios. Scale 1 will be used for most patients, with the exception of patients with an oxygen saturation target range of 88–92%, such as patients with hypercapnic respiratory failure.">
				>
				["at0030"] = <
					text = <"≥96">
					description = <"The oxygen saturation level is greater than/equal to 96%.">
				>
				["at0031"] = <
					text = <"94-95">
					description = <"The oxygen saturation level is between 94% and 95%.">
				>
				["at0032"] = <
					text = <"92-93">
					description = <"The oxygen saturation level is between 92% and 93%.">
				>
				["at0033"] = <
					text = <"≤91">
					description = <"The oxygen saturation level is less than/equal to 91%.">
				>
				["at0034"] = <
					text = <"Air or oxygen?">
					description = <"Is the patient receiving supplemental oxygen?">
				>
				["at0036"] = <
					text = <"Air">
					description = <"The patient is breathing room air; not receiving supplemental oxygen.">
				>
				["at0037"] = <
					text = <"Oxygen">
					description = <"The patient is receiving supplemental oxygen.">
				>
				["at0038"] = <
					text = <"≥39.1">
					description = <"The body temperature is greater than/equal to 39.1 degrees Celcius.">
				>
				["at0039"] = <
					text = <"≤35.0">
					description = <"The body temperature is less than/equal to 35 degrees Celcius.">
				>
				["at0045"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0046"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0047"] = <
					text = <"SpO₂ Scale 2">
					description = <"Category for the oxygen saturation measurement in patients with a target oxygen saturation range of 88–92%.">
					comment = <"'SpO₂ Scale 1' and 'SpO₂ Scale 2' are variations of the same physiological variable to be used in different clinical scenarios. Scale 2 will be used only in patients with an oxygen saturation target range of 88–92%, such as patients with hypercapnic respiratory failure. Otherwise use Scale 1.">
				>
				["at0048"] = <
					text = <"88-92 or ≥93 on air">
					description = <"The oxygen saturation level is between 88% and 92% or greater than/equal to 93% on air.">
				>
				["at0049"] = <
					text = <"86-87">
					description = <"The oxygen saturation level is between 86% and 87%.">
				>
				["at0050"] = <
					text = <"84-85">
					description = <"The oxygen saturation level is between 84% and 85%.">
				>
				["at0051"] = <
					text = <"≥97 on oxygen">
					description = <"The oxygen saturation level is greater than/equal to 97% on supplemental oxygen.">
				>
				["at0056"] = <
					text = <"Clinical risk category">
					description = <"Overall category representing the urgency and scale of the clinical response required in response to the physiological variables.">
				>
				["at0057"] = <
					text = <"Low">
					description = <"Ward-based response.">
				>
				["at0058"] = <
					text = <"Low-medium">
					description = <"Urgent ward-based response.">
				>
				["at0059"] = <
					text = <"Medium">
					description = <"Key threshold for urgent response.">
				>
				["at0060"] = <
					text = <"High">
					description = <"Urgent or emergency response.">
				>
				["at0061"] = <
					text = <"≤83">
					description = <"The oxygen saturation level is less/equal to 83%.">
				>
				["at0062"] = <
					text = <"93-94 on oxygen">
					description = <"The oxygen saturation level is between 93% and 94% on supplemental oxygen.">
				>
				["at0063"] = <
					text = <"95-96 on oxygen">
					description = <"The oxygen saturation level is between 95% and 96% on supplemental oxygen.">
				>
				["at0064"] = <
					text = <"≥25">
					description = <"The respiratory rate measurement is greater than/equal to 25 breaths/min.">
				>
				["at0065"] = <
					text = <"≥131">
					description = <"The pulse measurement is greater than/equal to 131 beats/min.">
				>
				["at0066"] = <
					text = <"38.1-39.0">
					description = <"The body temperature measurement is between 38.1 and 39.0 degrees Celsius.">
				>
				["at0067"] = <
					text = <"≥220">
					description = <"The systolic blood pressure measurement is greater than/equal to 220 mmHg.">
				>
				["at0068"] = <
					text = <"91-110">
					description = <"The pulse measurement is between 91 and 110 beats/min.">
				>
			>
		>
