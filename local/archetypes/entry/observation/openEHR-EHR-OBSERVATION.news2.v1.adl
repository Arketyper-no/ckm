				["at0028"] = <
					text = <"*Total score (en)">
					description = <"*The total sum of ordinal values associated with six relevant NEWS2 physiological parameters, plus the supplemental oxygen weighting score.(en)">
					comment = <"*The total score may be generated at run-time. (en)">
