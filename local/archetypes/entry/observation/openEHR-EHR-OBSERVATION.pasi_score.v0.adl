													0|[local::at0018],
													1|[local::at0019],
													2|[local::at0020],
													3|[local::at0021],
													4|[local::at0022]
												}
											}
											ELEMENT[at0023] occurrences matches {0..1} matches {    -- Body surface area involvement (A)
												value matches {
													0|[local::at0024],
													1|[local::at0025],
													2|[local::at0026],
													3|[local::at0027],
													4|[local::at0028],
													5|[local::at0029],
													6|[local::at0030]
												}
											}
											ELEMENT[at0038] occurrences matches {0..1} matches {    -- Score head region
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::380]>
														list = <
															["1"] = <
																units = <"1">
																magnitude = <|>=0.0|>
																precision = <|1|>
															>
														>
													>
												}
											}
										}
									}
									CLUSTER[at0032] occurrences matches {0..1} matches {    -- Trunk region (t)
