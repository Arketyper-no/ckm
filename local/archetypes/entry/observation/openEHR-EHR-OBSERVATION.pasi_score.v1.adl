	>

definition
	OBSERVATION[at0000] matches {	-- PASI Score
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {	-- *Point in Time event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {4..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {1..4} matches {	-- *Body Region(en)
										name matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0011, 	-- *Head(en)
													at0012, 	-- *Upper limbs(en)
													at0013, 	-- *Trunk(en)
													at0014]	-- *Lower limbs(en)
												}
											}
										}
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Rødhet
												value matches {
													0|[local::at0015], 	-- *None(en)
													1|[local::at0016], 	-- *Mild(en)
													2|[local::at0017], 	-- *Moderate(en)
													3|[local::at0018], 	-- *Severe(en)
													4|[local::at0019]  	-- *Very severe(en)
												}
											}
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Tykkelse
												value matches {
													0|[local::at0020], 	-- *None(en)
													1|[local::at0021], 	-- *Mild(en)
													2|[local::at0022], 	-- *Moderate(en)
													3|[local::at0023], 	-- *Severe(en)
													4|[local::at0024]  	-- *Very severe(en)
												}
											}
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- Skjelling
												value matches {
													0|[local::at0025], 	-- *None(en)
													1|[local::at0026], 	-- *Mild(en)
													2|[local::at0027], 	-- *Moderate(en)
													3|[local::at0028], 	-- *Severe(en)
													4|[local::at0029]  	-- *Very severe(en)
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Grad av involvering
												value matches {
													0|[local::at0030], 	-- None
													1|[local::at0031], 	-- 1-9%
													2|[local::at0032], 	-- 10-29%
													3|[local::at0033], 	-- 30-49%
													4|[local::at0034], 	-- 50-69%
													5|[local::at0035], 	-- 70-89%
													6|[local::at0037]  	-- 90-100%
												}
											}
											ELEMENT[at0036] occurrences matches {0..1} matches {	-- *Body Region Total(en)
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::380]>
														list = <
															["1"] = <
																units = <"1">
																magnitude = <|0.0..30.0|>
																precision = <|1|>
															>
														>
													>
												}
											}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Total Score
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
														magnitude = <|0.0..72.0|>
														precision = <|1|>
													>
												>
											>
										}
									}
									ELEMENT[at0010] occurrences matches {0..*} matches {	-- *Multimedia(en)
										value matches {
											DV_MULTIMEDIA matches {
												media_type matches {
													[openEHR::
													425, 
													426, 
													427, 
													428, 
													429, 
													401, 
													402, 
													404, 
													405, 
													406, 
													407, 
													414]
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


