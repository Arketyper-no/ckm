archetype (adl_version=1.4)
	openEHR-EHR-OBSERVATION.pasi_score.v1

concept
	[at0000]	-- PASI-score
language
	original_language = <[ISO_639-1::nb]>
	translations = <
		["en"] = <
			language = <[ISO_639-1::en]>
			author = <
				["name"] = <"Heather Leslie">
				["organisation"] = <"Ocean Informatics, Australia">
				["email"] = <"heather.leslie@oceaninformatics.com">
			>
		>
	>
description
	original_author = <
		["name"] = <"Silje Ljosland Bakke">
		["organisation"] = <"Helse Bergen HF, Norway">
		["email"] = <"silje.ljosland.bakke@helse-bergen.no">
		["date"] = <"2014-02-18">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Psoriasis Area Severity Index (PASI) is an index used to express the severity of psoriasis. It combines the severity (redness, thickness and scaling) and the percentage of affected area.">
			use = <"The PASI Score is a quantitateive measurement of the severity of psoriasis base on area coverage and plaque appearance. All components must be completed to calculate a valid total score.
This archetype enables recording of the relevant data elements. The application will need to appliy the algorithm correctly, including the weighting factors for each body region.">
			misuse = <"">
			copyright = <"© Nasjonal IKT HF">
		>
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Psoriasis Area Severity Index (PASI) er en indeks som brukes til å uttrykke alvorlighetsgraden av psoriasis. Det kombinerer alvorlighetsgrad (rødhet, tykkelse og skjelling) og andel av berørt område.">
			use = <"PASI-Score er en kvantitativ måling av alvorlighetsgraden av psoriasis basert på dekningsområde og plakk utseende. Alle komponenter må være gjennomført for å beregne en gyldig total poengsum. 

Denne arketypen muliggjør opptak av de aktuelle dataelementer. Applikasjonen må beregne algoritmen riktig, inklusive vekting for hver kroppsdel.">
			keywords = <"psoriasis", "PASI">
			misuse = <"">
			copyright = <"© Nasjonal IKT HF">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <"Bjørn Næss, DIPS ASA, Norway", "Heather Leslie, Ocean Informatics, Australia">
	other_details = <
		["references"] = <"http://www.hudlegekontoret.no/pasientinstruksjoner_a_aa/pasi_score

http://www.medicareaustralia.gov.au/provider/pbs/drugs1/files/ma_4178_PASI_calculation_and_whole_body_diagram.pdf

">
