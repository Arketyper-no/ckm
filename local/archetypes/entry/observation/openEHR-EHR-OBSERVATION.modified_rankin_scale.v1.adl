		["en"] = <
			items = <
				["at0000"] = <
					text = <"Modified Rankin Scale (mRS)">
					description = <"An assessment tool used to measure the degree of disability and level of assistance required after a stroke or another neurological event.">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any point in time event">
					description = <"Default, unspecified point in time event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0005"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0007"] = <
					text = <"Modified Rankin Scale">
					description = <"Degree of disability and level of assistance required after a stroke or another neurological event.">
				>
				["at0008"] = <
					text = <"No symptoms">
					description = <"No symptoms at all.">
				>
				["at0009"] = <
					text = <"No significant disability">
					description = <"Despite symptoms, able to carry out all usual duties and activities.">
				>
				["at0010"] = <
					text = <"Slight disability">
					description = <"Unable to perform all previous activities but able to look after own affairs without assistance.">
				>
				["at0011"] = <
					text = <"Moderate disability">
					description = <"Requiring some help but able to walk without assistance.">
				>
				["at0012"] = <
					text = <"Moderately severe disability">
					description = <"Unable to walk without assistance and unable to attend to own bodily needs without assistance.">
				>
				["at0013"] = <
					text = <"Severe disability">
					description = <"Bedridden, incontinent and requiring constant nursing care and attention.">
				>
				["at0014"] = <
					text = <"Death">
					description = <"Death.">
				>
			>
		>
