definition
	OBSERVATION[at0000] matches {    -- Neurologic Assessment in Neuro-Oncology (NANO) scale
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Gait
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											98|[local::at0047],
											99|[local::at0048]
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Strength
										value matches {
											0|[local::at0010],
											1|[local::at0011],
											2|[local::at0012],
											3|[local::at0013],
											98|[local::at0049],
											99|[local::at0050]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Ataxia (upper extremity)
										value matches {
											0|[local::at0015],
											1|[local::at0016],
											2|[local::at0017],
											98|[local::at0051],
											99|[local::at0052]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Sensation
										value matches {
											0|[local::at0019],
											1|[local::at0020],
											2|[local::at0021],
											98|[local::at0053],
											99|[local::at0054]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Visual fields
										value matches {
											0|[local::at0023],
											1|[local::at0024],
											2|[local::at0025],
											3|[local::at0026],
											98|[local::at0055],
											99|[local::at0056]
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- Facial strength
										value matches {
											0|[local::at0028],
											1|[local::at0029],
											2|[local::at0030],
											98|[local::at0057],
											99|[local::at0058]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- Language
										value matches {
											0|[local::at0032],
											1|[local::at0033],
											2|[local::at0034],
											3|[local::at0035],
											98|[local::at0059],
											99|[local::at0060]
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Level of consciousness
										value matches {
											0|[local::at0037],
											1|[local::at0038],
											2|[local::at0039],
											3|[local::at0040],
											98|[local::at0061],
											99|[local::at0062]
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {    -- Behavior
										value matches {
											0|[local::at0042],
											1|[local::at0043],
											2|[local::at0044],
											98|[local::at0063],
											99|[local::at0064]
										}
									}
									ELEMENT[at0066] occurrences matches {0..1} matches {    -- Overall NANO score
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0067,    -- Neurological response
													at0069,    -- Neurological stability
													at0068,    -- Neurological progression
													at0070,    -- Not assessed
													at0071]    -- Non-evaluable
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0046] matches {    -- ITEM_TREE
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0065] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}





