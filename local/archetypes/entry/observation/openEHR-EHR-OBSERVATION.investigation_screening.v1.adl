		["es"] = <
			items = <
				["at0000"] = <
					text = <"Cuestionario de detección de investigación diagnóstica">
					description = <"Serie de preguntas y respuestas asociadas, utilizadas para evaluar si se han realizado investigaciones diagnósticas.">
					comment = <"Las respuestas pueden ser autoinformadas.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
