				>
				["at0042"] = <
					text = <"Pressure">
					description = <"Measured intraocular pressure, using the method described in the 'Method' element.">
					comment = <"For devices that output multiple pressure values based on one measurement, this element is used for the uncorrected output. Use the 'Corrected Pressure' and 'Correction type' elements for outputs that have taken more variable such as corneal parameters into consideration. ">
				>
