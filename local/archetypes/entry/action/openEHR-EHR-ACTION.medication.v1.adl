definition
	ACTION[at0000] matches {	-- Medication action
		ism_transition matches {
			ISM_TRANSITION[at0042] matches {	-- Recommend medication
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::526]}
					}
