definition
	EVALUATION[at0000] matches {    -- Advance intervention decisions
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Intent of care
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Curative
									at0004,    -- Restorative
									at0005,    -- Supportive
									at0006,    -- Terminal
									at0007]    -- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0056] occurrences matches {0..1} matches {    -- Rationale
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Decisions description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- CPR decision
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0010,    -- Full CPR
									at0011,    -- Limited CPR
									at0012,    -- No CPR
									at0013]    -- Unknown
								}
							}
						}
					}
					CLUSTER[at0014] occurrences matches {0..*} matches {    -- Per intervention
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] matches {    -- Intervention
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0016,    -- Chest compressions
											at0017,    -- Defibrillation
											at0018,    -- Intubation
											at0019,    -- Invasive ventilation
											at0020,    -- Non-invasive ventilation
											at0021,    -- Peripheral intravenous line
											at0022,    -- Central venous line or any arterial line
											at0023,    -- Parenteral fluids
											at0024,    -- Parenteral or artificial nutrition
											at0025,    -- Arrest medications
											at0026,    -- Circulatory regulation medications
											at0027,    -- Antibiotics, antiviral or antifungal agents
											at0028,    -- Blood products
											at0029,    -- Chest tube insertion
											at0030,    -- Cardiac pacemaker
											at0031,    -- Dialysis
											at0032,    -- Transfer to hospital
											at0033]    -- Transfer to Intensive Care
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0034] occurrences matches {0..1} matches {    -- Decision
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0035,    -- Recommended
											at0036,    -- Conditional recommendation
											at0037,    -- Not recommended
											at0038]    -- Unknown
										}
									}
								}
							}
							ELEMENT[at0039] occurrences matches {0..*} matches {    -- Precondition
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0040] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {    -- Patient awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {    -- Carer awareness
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {    -- Overall comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0045] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0046] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Valid period start
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0048] occurrences matches {0..1} matches {    -- Valid period end
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Review due
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0050] occurrences matches {0..1} matches {    -- Mandate
						value matches {
							DV_TEXT matches {*}
							DV_EHR_URI matches {*}
						}
					}
					allow_archetype CLUSTER[at0051] occurrences matches {0..*} matches {    -- Digital representation
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia_source\.v0/}
					}
					CLUSTER[at0052] occurrences matches {0..1} matches {    -- Document location
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0053] occurrences matches {0..1} matches {    -- Location
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							allow_archetype CLUSTER[at0054] occurrences matches {0..*} matches {    -- Copy holder
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
					allow_archetype CLUSTER[at0055] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Advance intervention decisions">
					description = <"Anticipatory decisions about the overall intent of care and possible interventions (including treatments, activities, and diagnostic or therapeutic procedures), asserted by a clinician.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Intent of care">
					description = <"The overall intent for future care and treatment for the individual.">
					comment = <"The proposed value set is based on the MJA reference. 
Coding the intent of care with an external terminology is preferred, where possible.">
				>
				["at0003"] = <
					text = <"Curative">
					description = <"The condition is curable; all appropriate life-prolonging treatment will be offered.">
				>
				["at0004"] = <
					text = <"Restorative">
					description = <"The condition is treatable with potential for restoration to the previous state of health or a prolonged remission; all appropriate life-prolonging treatment will be offered.">
				>
				["at0005"] = <
					text = <"Supportive">
					description = <"The condition is incurable and progressive; active treatment of the underlying disease may be appropriate for specific symptom relief and/or short term life expectancy gains; focused on comfort, quality of life and dignity. Also referred to as 'Palliative'.">
				>
				["at0006"] = <
					text = <"Terminal">
					description = <"Life expectancy is short (hours or days); no active treatment of the underlying disease or treatment-related harm; focus on comfort, quality of life and dignity.">
				>
				["at0007"] = <
					text = <"Unknown">
					description = <"The intent of care decision is not known.">
				>
				["at0008"] = <
					text = <"Decisions description">
					description = <"Narrative description about all advance intervention decisions, including the CPR decision.">
					comment = <"May be used to record a narrative overview of the advance intervention decision that reflect the specific circumstances for the individual. This includes any limitations, or conditional circumstances applicable, to interventions. This description may or may not be supported by structured data, recorded using the ‘Per intervention’ cluster. If both this narrative description and ‘Per intervention’ structured data is recorded, caution is required as there is a potential risk of divergence. This data element may be used to capture legacy data that is not available in a structured format.">
				>
				["at0009"] = <
					text = <"CPR decision">
					description = <"Decision about the extent of cardiopulmonary resuscitation (CPR) intervention appropriate for this individual.">
				>
				["at0010"] = <
					text = <"Full CPR">
					description = <"All appropriate cardiopulmonary resuscitation treatments should be attempted to save or prolong life.">
				>
				["at0011"] = <
					text = <"Limited CPR">
					description = <"Cardiopulmonary resuscitation treatments should be attempted, as specified in the 'Decisions description' narrative or as structured data in the 'Per intervention' cluster.">
				>
				["at0012"] = <
					text = <"No CPR">
					description = <"No cardiopulmonary resuscitation treatments should be attempted to save or prolong life. Also known as 'Do not resuscitate (DNR)'; 'Do not attempt resuscitation (DNAR)'; 'Not for resuscitation (NFR)' or similar.">
				>
				["at0013"] = <
					text = <"Unknown">
					description = <"The CPR decision is not known.">
				>
				["at0014"] = <
					text = <"Per intervention">
					description = <"Details of the decision about possible treatments or activities that may be life-saving, life-prolonging or cause undesirable side effects.">
				>
				["at0015"] = <
					text = <"Intervention">
					description = <"Name of the possible treatment, procedure or activity.">
					comment = <"While this archetype is intended for all clinical decisions about possible future interventions, the proposed DV_CODED_TEXT value set for the 'Intervention' data element is deliberately not intended to be exhaustive. Instead it has been designed to be inclusive of most of the common life-saving treatments within an emergency context. The alternate choice of the DV_TEXT option can be used to add a local value set or bind to an alternative external value set within a template.">
				>
				["at0016"] = <
					text = <"Chest compressions">
					description = <"Technique used to manually pump blood around the body during a cardiac arrest.">
				>
				["at0017"] = <
					text = <"Defibrillation">
					description = <"Use of electric shock to reset the electrical state of the heart.">
				>
				["at0018"] = <
					text = <"Intubation">
					description = <"Placement of a tube into the trachea to maintain an open airway, including a tracheostomy.">
				>
				["at0019"] = <
					text = <"Invasive ventilation">
					description = <"Artificial ventilation where invasive means are used to assist or replace spontaneous breathing.">
				>
				["at0020"] = <
					text = <"Non-invasive ventilation">
					description = <"Artificial ventilation where non-invasive means are used to assist or replace spontaneous breathing.">
				>
				["at0021"] = <
					text = <"Peripheral intravenous line">
					description = <"Insertion of a peripheral intravenous line.">
				>
				["at0022"] = <
					text = <"Central venous line or any arterial line">
					description = <"Insertion of a central venous line or any arterial line.">
				>
				["at0023"] = <
					text = <"Parenteral fluids">
					description = <"Maintenance of hydration by means other than eating or drinking.">
				>
				["at0024"] = <
					text = <"Parenteral or artificial nutrition">
					description = <"Provision of nutrients by means other than eating or drinking.">
				>
				["at0025"] = <
					text = <"Arrest medications">
					description = <"Medications used during an emergency resuscitation to prolong life. For example: adrenaline, atropine, sodium bicarbonate, calcium, other vasoactive medications.">
				>
				["at0026"] = <
					text = <"Circulatory regulation medications">
					description = <"Medication that supports heart function, including but not limited to: vasopressors, inotropes and chronotropes.">
				>
				["at0027"] = <
					text = <"Antibiotics, antiviral or antifungal agents">
					description = <"Medications to treat infections.">
				>
				["at0028"] = <
					text = <"Blood products">
					description = <"Therapeutic substance prepared from human blood including, but not limited to: whole blood; blood components; and plasma derivatives.">
				>
				["at0029"] = <
					text = <"Chest tube insertion">
					description = <"Insertion of a tube into the pleural space.">
				>
				["at0030"] = <
					text = <"Cardiac pacemaker">
					description = <"Insertion of an artificial pacemaker to control the heart beat.">
				>
				["at0031"] = <
					text = <"Dialysis">
					description = <"Process of removing excess water, solutes, and toxins from the blood.">
				>
				["at0032"] = <
					text = <"Transfer to hospital">
					description = <"Transfer to hospital for assessment or admission.">
				>
				["at0033"] = <
					text = <"Transfer to Intensive Care">
					description = <"Transfer and admission to an intensive care or high dependency unit.">
				>
				["at0034"] = <
					text = <"Decision">
					description = <"Decision about the identified intervention.">
				>
				["at0035"] = <
					text = <"Recommended">
					description = <"The intervention is recommended.">
				>
				["at0036"] = <
					text = <"Conditional recommendation">
					description = <"The intervention is recommended only in specific circumstances, captured using the 'Preconditions' data element.">
				>
				["at0037"] = <
					text = <"Not recommended">
					description = <"The intervention is not recommended.">
				>
				["at0038"] = <
					text = <"Unknown">
					description = <"The intervention decision is not known.">
				>
				["at0039"] = <
					text = <"Precondition">
					description = <"Description of the circumstance/s in which a 'Conditional recommendation' is applicable.">
					comment = <"This data element is only relevant if the 'Conditional recommendation' option is selected in the 'Decision' data element. For example:' \"Transfer to hospital is only applicable in the circumstances where the individual suffers a fracture and requires immobilization for pain relief.#">
				>
				["at0040"] = <
					text = <"Comment">
					description = <"Additional narrative about the intervention decision, not captured in other fields.">
					comment = <"For example: the justification or rationale for the intervention decision.">
				>
				["at0041"] = <
					text = <"Additional details">
					description = <"Additional structured details about advance intervention decisions.">
					comment = <"For example: people involved in the decision-making process, if it is not possible to use the reference model participations; or additional locally relevant considerations.">
				>
				["at0042"] = <
					text = <"Patient awareness">
					description = <"Narrative description about awareness of the Advance intervention decisions by the individual.">
					comment = <"For example: the capacity of the patient to understand the decisions.">
				>
				["at0043"] = <
					text = <"Carer awareness">
					description = <"Narrative description about awareness of the Advance intervention decisions by family, carers or legal proxy.">
					comment = <"For example: the individual has no family.">
				>
				["at0044"] = <
					text = <"Overall comment">
					description = <"Additional narrative about all advance intervention decisions, not captured in other fields.">
				>
				["at0045"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0046"] = <
					text = <"Last updated">
					description = <"The date and/or time when the Advance intervention decisions were last updated.">
				>
				["at0047"] = <
					text = <"Valid period start">
					description = <"The date/time that marks the beginning of the valid period of time for the Advance intervention decisions.">
				>
				["at0048"] = <
					text = <"Valid period end">
					description = <"The date/time that marks the conclusion of the valid period of time for the Advance intervention decisions.">
					comment = <"'Valid period end' may often overlap with 'Review due date'. However, they may need to be recorded separately in circumstances where a document has an extended period of validity but requires an interim review. That may be due to changed personal circumstances/events or local policy.">
				>
				["at0049"] = <
					text = <"Review due">
					description = <"The date when review of the Advance intervention decisions are due.">
				>
				["at0050"] = <
					text = <"Mandate">
					description = <"Description of any legislation or other authoritative guidance that apply.">
					comment = <"For example: an advance care directive.">
				>
				["at0051"] = <
					text = <"Digital representation">
					description = <"Digital document, image or video representing these Advance intervention decisions.">
				>
				["at0052"] = <
					text = <"Document location">
					description = <"Information about the physical or digital location of the advance intervention decisions record.">
				>
				["at0053"] = <
					text = <"Location">
					description = <"Physical or digital location of the Advance intervention decisions record.">
				>
				["at0054"] = <
					text = <"Copy holder">
