definition
	EVALUATION[at0000] matches {	-- Absence of Information
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Absence Statement
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0003] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Date Verified
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Absence of Information">
					description = <"Positive statement/s about information that is not available within the health record at a specific point in time.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Absence Statement">
					description = <"Positive statement that no information is available.">
					comment = <"For example: \"No information available about adverse reactions\"; No information available about problems or diagnoses\"; \"No information available about previous procedures performed\"; or \"No information available about medications used\".">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Date Verified">
					description = <"The date at which the information was deemed to be absent.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Fravær av informasjon">
					description = <"Eksplisitt registrering av at informasjon ikke er tilgjengelig i journalen på et gitt tidspunkt">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Utsagn om fravær">
					description = <"Eksplisitt utsagn/bekreftelse på at informasjon ikke er tilgjengelig eller mulig å oppdrive">
					comment = <"For eksempel: \"Ikke tilgjengelig informasjon om Cave/Allergi\", \"Ikke tilgjengelig informasjon om problemer eller diagnoser\", \"Ikke tilgjengelig informasjon om tidligere gjennomførte prosedyrer\", eller \"Ikke tilgjengelig informasjon om medikamenter i bruk\".">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Dato bekreftet">
					description = <"Datoen informasjonen ble ansett å være fraværende.">
				>
			>
		>
	>
