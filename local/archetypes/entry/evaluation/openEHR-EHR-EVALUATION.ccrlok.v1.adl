definition
	EVALUATION[at0000] matches {	-- Lokalisasjon primærtumor
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {1; unordered} matches {
					ELEMENT[at0002] matches {	-- Lokalisasjon av primærtumor
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003, 	-- Cøkum (C18.0)
									at0004, 	-- Appendix (C18.1)
									at0005, 	-- Ascendens (C18.3)
									at0018, 	-- Høyre fleksur (C18.3
									at0019, 	-- Transversum (C18.4)
									at0020, 	-- Venstre fleksur (C18.5)
									at0021, 	-- Descendens (C18.6)
									at0022, 	-- Sigmoideum, ≥ 20 cm (C18.7)
									at0023, 	-- Tykktarm uspesifisert (C18.9)
									at0024, 	-- Tykktarm, overlappende (C18.8)
									at0025, 	-- Rectosigmoid, ≥ 16 og < 20 cm (C19)
									at0028]	-- Rectum > 16 cm (C20)
								}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- Avstand fra analåpning til tumor
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"cm">
										magnitude = <|0.0..99.9|>
										precision = <|1|>
									>
								>
							>
						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {	-- Avstand fra M.puborectalis til tumor
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"cm">
										magnitude = <|0.0..99.9|>
										precision = <|1|>
									>
								>
							>
						}
					}
				}
			}
		}
	}


