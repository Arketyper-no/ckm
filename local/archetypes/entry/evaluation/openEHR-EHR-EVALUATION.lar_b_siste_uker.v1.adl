definition
	EVALUATION[at0000] matches {	-- LAR siste 4 uker
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0002] occurrences matches {0..1} matches {	-- B1 Behandling og rådgivning siste 4 uker
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0003] occurrences matches {0..1} matches {	-- a) Målsetning for behandling
								value matches {
									0|[local::at0004], 	-- Rehab. m/rusfrihet
									1|[local::at0005], 	-- Stabilisering/uten krav til rusfrihet
									9|[local::at0006]  	-- Ikke avtalt
								}
							}
							ELEMENT[at0007] occurrences matches {0..1} matches {	-- b) Hovedansvar i Spesialisthelsetjenesten/LAR
								value matches {
									0|[local::at0008], 	-- Hovedansvar i spesialisthelsetjenesten/LAR (ikke overført)
									1|[local::at0009], 	-- Hovedansvar i førstelinjen (overført)
									9|[local::at0011], 	-- Ukjent
									10|[local::at0010]  	-- Annet
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- c) Gjennomført rehabilitering
								value matches {
									0|[local::at0013], 	-- Nei
									1|[local::at0014], 	-- Ja
									9|[local::at0015]  	-- Ukjent
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {	-- d) Er pasienten i psykatrisk behandling
								value matches {
									0|[local::at0017], 	-- Nei
									1|[local::at0018], 	-- Ja
									9|[local::at0019]  	-- Ukjent
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- e) Er det utarbeidet individuell plan
								value matches {
									0|[local::at0021], 	-- Nei
									1|[local::at0022], 	-- Ja
									9|[local::at0023]  	-- Ukjent
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {	-- f) Har pasienten systematisk psykoterapeutisk behandling?
								value matches {
									0|[local::at0025], 	-- Nei
									1|[local::at0026], 	-- Ja
									9|[local::at0027]  	-- Ukjent
								}
							}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- B2 Har det vært avholdt ansvarsgrupper siste 3 mnd?
						value matches {
							0|[local::at0031], 	-- Nei
							1|[local::at0032], 	-- Ja
							9|[local::at0033]  	-- Ukjent
						}
					}
					CLUSTER[at0034] occurrences matches {0..1} matches {	-- B3 Psykiske vansker siste 4 uker
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0035] occurrences matches {0..1} matches {	-- a) Alvorlig depresjon
								value matches {
									0|[local::at0036], 	-- Nei
									1|[local::at0037], 	-- Ja
									9|[local::at0038]  	-- Ukjent
								}
							}
							ELEMENT[at0039] occurrences matches {0..1} matches {	-- b) Alvorlig angst
								value matches {
									0|[local::at0040], 	-- Nei
									1|[local::at0041], 	-- Ja
									9|[local::at0042]  	-- Ukjent
								}
							}
							ELEMENT[at0043] occurrences matches {0..1} matches {	-- c) Vrangforestillinger/Hallusinasjoner
								value matches {
									0|[local::at0044], 	-- Nei
									1|[local::at0045], 	-- Ja
									9|[local::at0046]  	-- Ukjent
								}
							}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {	-- B4 Kroppslige skader/sykdommer som påvirker livsførsel eller livskvalitet siste 4 uker
						value matches {
							0|[local::at0048], 	-- Nei
							1|[local::at0049], 	-- Ja
							9|[local::at0050]  	-- Ukjent
						}
					}
					CLUSTER[at0051] occurrences matches {0..1} matches {	-- B5 Rapportert rusmiddelbruk siste 4 uker
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0052] occurrences matches {0..1} matches {	-- a) Morfinstoffer, særlig heroin men også andre
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0053] occurrences matches {0..1} matches {	-- b) Cannabis
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0054] occurrences matches {0..1} matches {	-- c) Benzodiazopiner eller lignende
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0055] occurrences matches {0..1} matches {	-- d) Sentralstimulerende midler
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0056] occurrences matches {0..1} matches {	-- e) Alkoholbruk som gir påvisbare problemer
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
					ELEMENT[at0057] occurrences matches {0..1} matches {	-- B6 Hyppighet av stoff- og alkoholbruk siste 4 uker
						value matches {
							0|[local::at0058], 	-- Aldri
							1|[local::at0059], 	-- Få enkeltepisoder
							2|[local::at0060], 	-- Regelmessig bruk (mer enn 4 ggr)
							9|[local::at0061]  	-- Ukjent
						}
					}
					ELEMENT[at0062] occurrences matches {0..1} matches {	-- B7 Alvorlighet av stoff- og alkoholbruk siste 4 uker 
						value matches {
							0|[local::at0063], 	-- Aldri ruspåvirket
							1|[local::at0064], 	-- Av og til ruspåvirket
							2|[local::at0065], 	-- Avhengighetspreget, rusdominert
							9|[local::at0066]  	-- Ukjent
						}
					}
				}
			}
		}
	}


