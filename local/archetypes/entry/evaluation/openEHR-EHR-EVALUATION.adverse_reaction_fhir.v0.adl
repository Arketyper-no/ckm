				["at0066"] = <
					text = <"Motbevist">
					description = <"En tendens til reaksjon på den identifiserte substansen har blitt revurdert ved testing og/eller re-eksponering, og er motbevist med høyt nivå av sikkerhet.">
				>
				["at0067"] = <
					text = <"Avsluttet">
