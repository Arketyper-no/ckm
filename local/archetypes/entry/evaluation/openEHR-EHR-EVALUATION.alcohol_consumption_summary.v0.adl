												defining_code matches {
													[local::
													at0115,    -- Beer
													at0116,    -- Wine
													at0117,    -- Cider
													at0118,    -- Mead
													at0119,    -- Pulque
													at0120,    -- Spirits
													at0121]    -- Fortified wine
												}
											}
										}
									}
									ELEMENT[at0053] occurrences matches {0..1} matches {    -- Description
										value matches {
