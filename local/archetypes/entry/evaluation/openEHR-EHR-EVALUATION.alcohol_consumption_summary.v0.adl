											at0083,    -- Daily
											at0084]    -- Non-daily
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0097] occurrences matches {0..1} matches {    -- Binge drinking frequency
								value matches {
									C_DV_QUANTITY <
										
										list = <
											["1"] = <
												units = <"1/wk">
												magnitude = <|0.0..7.0|>
												precision = <|0|>
											>
											["2"] = <
												units = <"1/mo">
												magnitude = <|0.0..31.0|>
												precision = <|0|>
											>
											["3"] = <
												units = <"1/a">
												magnitude = <|0.0..365.0|>
												precision = <|0|>
											>
										>
									>
									DV_INTERVAL<DV_QUANTITY> matches {
										lower matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"1/wk">
														magnitude = <|0.0..7.0|>
														precision = <|0|>
													>
													["2"] = <
														units = <"1/mo">
														magnitude = <|0.0..31.0|>
														precision = <|0|>
													>
													["3"] = <
														units = <"1/a">
														magnitude = <|0.0..365.0|>
														precision = <|0|>
													>
												>
											>
										}
										upper matches {
											C_DV_QUANTITY <
												
												list = <
													["1"] = <
														units = <"1/wk">
														magnitude = <|0.0..7.0|>
														precision = <|0|>
													>
													["2"] = <
														units = <"1/mo">
														magnitude = <|0.0..31.0|>
														precision = <|0|>
													>
													["3"] = <
														units = <"1/a">
														magnitude = <|0.0..365.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
								}
							}
