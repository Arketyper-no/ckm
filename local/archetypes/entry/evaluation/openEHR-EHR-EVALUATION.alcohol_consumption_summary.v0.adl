												units = <"1/wk">
												magnitude = <|0.0..<7.0|>
												precision = <|0|>
											>
											["2"] = <
												units = <"1/mo">
												magnitude = <|0.0..<31.0|>
												precision = <|0|>
											>
										>
									>
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {    -- Typical consumption (alcohol units)
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::382]>
										list = <
											["1"] = <
												units = <"1/d">
												magnitude = <|>=0.0|>
												precision = <|1|>
											>
											["2"] = <
												units = <"1/wk">
												magnitude = <|>=0.0|>
												precision = <|1|>
											>
											["3"] = <
