definition
	EVALUATION[at0000.1] matches {	-- Exclusion of a Procedure
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002.1] occurrences matches {1..*} matches {	-- Exclusion statement
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003.1] occurrences matches {0..*} matches {	-- Procedure
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0006] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"***Exclusion statement(en)(en)(en)">
					description = <"***Statement about clinical conditions, issues or concerns that are either not currently present and/or have not been present in the past.(en)(en)(en)">
				>
				["at0000.1"] = <
					text = <"Exclusion of a Procedure">
					description = <"A statement about procedures that have never been noted by the individual or recognised by a clinician.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"***Exclusion statement(en)(en)(en)">
					description = <"***Statement of exclusion of a condition, issue or concern.(en)(en)(en)">
				>
				["at0002.1"] = <
					text = <"Exclusion statement">
					description = <"Statement of exclusion of procedures.">
					comment = <"Use to record a statement about procedures that have never been noted by the individual or recognised by a clinician. This statement can support recording general statements such as \"No earlier surgery related to...\" or \"Never experienced ...\". Or it can support more exacting statements about a specified object such as \"Never been through...\" where the 'Procedure' identifies the precise procedure.">
				>
				["at0003"] = <
					text = <"***Specific item(en)(en)(en)">
					description = <"***Identification of the specific clinical item to which the 'Exclusion statement' applies.(en)(en)(en)">
				>
				["at0003.1"] = <
					text = <"Procedure">
					description = <"Identification of the 'Procedure' to which the 'Exclusion statement' applies.">
					comment = <"This data element is used to enable exact statements like: 'Never gone through an appendectomy'. In this example, 'appendectomy' would be an 
example of the 'Procedure' and the rest of the example phrase would be carried in the 'Exclusion statement'. Another possible use case might be 'never performed a gastroscopy'. Please note: if coordinated phrases such as 'Never been through surgery in the abdomen' is available, 
then this can be used as an alternative in the 'Exclusion statement' alone.">
				>
				["at0004"] = <
					text = <"Last updated">
					description = <"The date on which this exclusion was last updated.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Comment">
					description = <"Additional narrative about the Exclusion not captured in other fields.">
				>
				["at0008"] = <
					text = <"Extension">
					description = <"Additional information required to capture local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Eksklusjonsutsagn">
					description = <"Utsagn om kliniske tilstander, forhold eller bekymringer som ikke er tilstede nå og/eller ikke har vært tilstede tidligere.">
				>
				["at0000.1"] = <
					text = <"Eksklusjon av en prosedyre">
					description = <"Et utsagn om prosedyrer som aldri er lagt merke til av individet eller bekreftet av en kliniker.
">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Eksklusjonsutsagn">
					description = <"Utsagn om eksklusjon av en klinisk tilstand, forhold eller bekymring.">
					comment = <"Brukes til å fange opp all helseinformasjon som eksplisitt skal registreres som ikke tilstede nå og/eller ikke til stede tidligere. Utsagnet støtter registrering av generelle utsagn som \"Ingen tidlgere...\" eller \"Ikke kjent...\". Den kan også støtte mer krevende utsagn om et spesifikt objekt som \"Ingen kjent historie om...\" der \"Spesifikt objekt\" identifiserer den aktuelle kliniske tilstanden, forholdet eller bekymringen. For eksplisitt modellerte eksempler, se spesialiseringer for legemidler, uønskede reaksjoner, problemer/diagnoser og prosedyrer.">
				>
				["at0002.1"] = <
					text = <"Eksklusjonsutsagn">
					description = <"Utsagn om eksklusjon av prosedyrer.">
					comment = <"Bruk for å registrere et utsagn om prosedyrer som aldri er lagt merke til av individet eller bekreftet av en kliniker.
Utsagnet støtter registrering av generelle utsagn som \"Ingen tidligere kirurgi...\" eller \"Ikke kjent...\". Den kan også støtte mer krevende utsagn om et spesifikt objekt som \"Ingen kjent forekomst av ...\" der \"Prosedyre\" identifiserer den aktuelle prosedyren.">
				>
				["at0003"] = <
					text = <"Spesifikt objekt">
					description = <"Identifikasjon av det spesifikke objektet \"Eksklusjonsutsagnet\" gjelder.">
					comment = <"I en generisk kontekst er det ikke enkelt å identifisere et vanlig brukt eksempel, men om en ser på spesialiseringer av denne arketypen vil dette dataelementet brukes for å gi presise utsagn som: \"Kortikosteroider har aldri blitt brukt\", \"Det har aldri oppstått en allergisk reaksjon til penicillin\", \"Ingen familiær forekomst av hjertesykdom\", \"Aldri hatt diabetes\". Fra disse eksemplene vil \"kortikosteroider\", \"penicillin\", \"hjertesykdom\" og \"diabetes\" være eksempler på \"Spesifikt objekt\", og resten av setningene registreres i \"Eksklusjonsutsagn\". Andre mulige bruksområder kan være: \"Utelukkelse av graviditet\", \"Aldri tatt røntgen\" eller \"Aldri mottatt blodoverføring\".
Merk at om prekoordinerte fraser som: \"Ingen kjent pencillinallergi\" finnes kan dette brukes som et alternativ i datalemententet \"Eksklusjonsutsagn\".">
				>
				["at0003.1"] = <
					text = <"Prosedyre">
					description = <"Identifikasjon av den spesifikke prosedyren \"Eksklusjonsutsagn\" gjelder.">
					comment = <"Dette dataelementet brukes for å gi presise utsagn som f.eks. \"Aldri gjennomgått appendektomi\". I dette eksempelet er \"appendektomi\" et eksempel på \"Prosedyre\", og resten av setningen registreres i \"Eksklusjonsutsagn\". Et annet mulig bruksområde kan være \"Ikke utført gastroskopi\". Merk at om prekoordinerte fraser som \"Aldri gjennomgått kirurgi i abdomen\" finnes, kan dette brukes som et alternativ i datalemententet \"Eksklusjonsutsagn\".">
				>
				["at0004"] = <
					text = <"Sist oppdatert">
					description = <"Dato da denne eksklusjonen sist ble oppdatert.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om eksklusjonen som ikke fanges opp i andre felter.">
				>
				["at0008"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som er nødvendig for å registrere lokalt innhold/kontekst, eller for å sammenstille med andre referansemodeller/formalismer.">
					comment = <"F.eks lokale informasjonskrav eller ekstra metadata for å tilpasse med FHIR eller CIMI ekvivalenter.">
				>
			>
		>
	>
