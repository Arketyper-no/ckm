definition
	EVALUATION[at0000] matches {	-- Contraindication
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] matches {	-- Contraindication
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Absolute?
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Rationale
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0006] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Review date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Contraindication">
					description = <"Identification of a treatment, test or procedure which should not be provided to the subject of care, for clinical reasons.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Contraindication">
					description = <"Identification of a contraindication to a treatment, test or procedure, including a class of medications or vaccines.">
					comment = <"Coding of the identified 'Contraindication' with a terminology is desirable, where possible.">
				>
				["at0003"] = <
					text = <"Rationale">
					description = <"Description of evidence or rationale for the contraindication.">
					comment = <"Please note: an optional URI link to evidence within the health record is also permitted using Reference Model attributes. As this URI link may not be accessible from a message or by receiving clinical system it is desirable that a narrative description of the rationale should be explicitly stated. ">
				>
				["at0004"] = <
					text = <"Last updated">
					description = <"The date this contraindication was last updated.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Absolute?">
					description = <"Is this an absolute contraindication?">
					comment = <"Record as TRUE if the recording clinician regards that there are no reasonable circumstances for the identified contraindication to be administered or performed.">
				>
				["at0008"] = <
					text = <"Comment">
					description = <"Additional narrative about the contraindication, not captured in other fields.">
				>
				["at0009"] = <
					text = <"Review date">
					description = <"Date when due for review by a clinician.">
					comment = <"In some circumstances, contraindications are not intended to be indefinite or lifelong, and this contraindication should be reconsidered in the context of changing clinical circumstances. For example: if a family member is no longer taking immunosuppressive therapy, then live vaccines could safely be administered to the subject of care again and the contraindication is effectively obsolete.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Kontraindikasjon">
					description = <"Angivelse av behandling, prøve eller prosedyre som ikke bør utføres av kliniske årsaker på pasienten.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Kontraindikasjon">
					description = <"Angivelse av en kontraindikasjon til behandling, prøve eller prosedyre. En kan også registrere legemiddel- eller vaksineklasse.">
					comment = <"Koding av \"Kontraindikasjon\" med en terminologi er ønskelig om mulig.">
				>
				["at0003"] = <
					text = <"Begrunnelse">
					description = <"Beskrivelse av bevis eller logisk begrunnelse for kontraindikasjonen.">
					comment = <"Merk: En valgfri URI for å lenke til kilden i journalen er også tillatt via referansemodellens attributter. Da en ikke er sikker på om denne lenken er tilgjengelig via meldinger eller i et mottakende klinisk system er det ønskelig at en fritekstbeskrivelse av beskrivelsen registreres eksplisitt.">
				>
				["at0004"] = <
					text = <"Sist oppdatert.">
					description = <"Dato da denne kontraindikasjonen sist ble oppdatert.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Absolutt?">
					description = <"Er kontraindikasjonen absolutt?">
					comment = <"Registrer som SANN om registrerende kliniker anser at det ikke finnes noen rimelige omstendigheter hvor identifisert kontraindikasjon skal administreres eller utføres.">
				>
				["at0008"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om kontraindikasjonen som ikke kan registreres i andre felt.">
				>
				["at0009"] = <
					text = <"Dato for gjennomgang">
					description = <"Dato når kontraindikasjonen skal gjennomgås av en kliniker.">
					comment = <"Noen ganger er kontraindikasjonen ikke tenkt å være livslang og kontraindikasjonen bør revurderes i lys av endrede kliniske omstendigheter. For eksempel: Om et familiemedlem ikke lenger tar immunsupprimerende behandling, kan vaksinen trygt administreres til pasienten igjen og kontraindikasjonen er ikke lenger aktuell.">
				>
			>
		>
	>
