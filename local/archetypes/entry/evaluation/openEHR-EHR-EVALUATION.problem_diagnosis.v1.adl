definition
	EVALUATION[at0000] matches {	-- Problem/diagnose
		data matches {
			ITEM_TREE[at0001] matches {	-- structure
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[at0002] matches {	-- Problem/diagnosenavn
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Klinisk beskrivelse
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Anatomisk sted
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0039] occurrences matches {0..1} matches {	-- Komplekst anatomisk sted
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.relative_location(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Dato/tid for klinisk stadfestelse
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Alder for klinisk stadfestelse
						value matches {
							DV_DURATION matches {
								value matches {PMWDTHM}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Alvorlighetsgrad
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0047, 	-- Mild
									at0048, 	-- Moderat
									at0049]	-- Alvorlig
								}
							}
						}
					}
					allow_archetype CLUSTER[at0043] occurrences matches {0..*} matches {	-- Spesifikke detaljer
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0072] occurrences matches {0..1} matches {	-- Forløpsbeskrivelse
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- Dato/tid for bedring
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Alder ved bedring
						value matches {
							DV_DURATION matches {
								value matches {PMWDTHM}
							}
						}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {	-- Relaterte saker
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0029] occurrences matches {0..1} matches {	-- Forhold
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0040, 	-- Årsak
											at0041]	-- Sekvele eller senvirkninger
										}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {	-- Sak
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							ELEMENT[at0044] occurrences matches {0..1} matches {	-- Beskrivelse
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0042] occurrences matches {0..1} matches {	-- Diagnostiske kriterier
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {	-- Status
						include
							archetype_id/value matches {/.*/}
						exclude
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.problem_status(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0069] occurrences matches {0..1} matches {	-- Kommentar
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0032] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0070] occurrences matches {0..1} matches {	-- Sist oppdatert
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0071] occurrences matches {0..*} matches {	-- Utvidelse
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["pt-br"] = <
			items = <
				["at0000"] = <
