				>
				["at0031"] = <
					text = <"*Age at resolution(en)">
					description = <"*Age of the individual at the time of resolution or remission for this problem or diagnosis as determined by a healthcare professional.(en)">
					comment = <"*Age can be the preferred data element on which to base decision support in situations where approximations of age based on calculations are not accurate enough to drive decision support, for example in infants under one year of age.(en)">
