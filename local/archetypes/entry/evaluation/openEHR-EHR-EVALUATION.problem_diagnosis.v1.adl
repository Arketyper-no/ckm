		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Problem/diagnose">
					description = <"Oppsummerende detaljer om en identifisert helsetilstand, skade eller avvik fra normalfunksjon som kan påvirke et individs fysiske, mentale og/eller sosiale velvære.">
					comment = <"Det er i praksis ikke lett å oppnå et klart skille mellom et problem og en diagnose. I klinisk dokumentasjon med denne arketypen er problem og diagnose regnet som del av et spekter, der det er økende krav til detaljer og støttende funn for å støtte en diagnose.">
				>
				["at0001"] = <
					text = <"structure">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Problem/diagnosenavn">
					description = <"Navn på det grunnleggende problemet eller diagnosen.">
					comment = <"Koding av navnet på problemet eller diagnosen ved hjelp av en terminologi er foretrukket, om mulig.">
				>
				["at0003"] = <
					text = <"Dato/tid for klinisk stadfestelse">
					description = <"Estimert eller faktisk dato/tid da diagnosen ble stadfestet av helsepersonell.">
					comment = <"Det er mulig å registrere deldatoer.">
				>
				["at0004"] = <
					text = <"Alder for klinisk stadfestelse">
					description = <"Individets alder da diagnosen ble stadfestet av helsepersonell.">
					comment = <"Alder kan være det foretrukne dataelementet å basere beslutningsstøtte på i situasjoner der anslag av alder ikke er nøyaktig nok som grunnlag for beslutningsstøtte, f.eks. hos spedbarn under ett år.">
				>
				["at0005"] = <
					text = <"Alvorlighetsgrad">
					description = <"En vurdering av den overordnede alvorlighetsgraden av problemet eller diagnosen.">
					comment = <"Dersom alvorlighetsgrad inkluderes i feltet \"Problem/diagnosenavn\" via prekoordinerte koder blir dette dataelementet overflødig. Merk: Mer spesifikk gradering av alvorlighetsgrad kan registreres ved å bruke SLOTet \"Spesifikke detaljer\".">
				>
				["at0009"] = <
					text = <"Klinisk beskrivelse">
					description = <"Narrativ beskrivelse av problemet eller diagnosen.">
				>
				["at0012"] = <
					text = <"Anatomisk lokalisering">
					description = <"Enkel registrering av hvor på kroppen problemet eller diagnosen opptrer.">
					comment = <"Forekomster for dette dataelementet er satt ubegrenset for å tillate kliniske scenarier som for eksempel å beskrive et utslett som opptrer flere steder på kroppen, men der alle andre attributter er identiske. Dersom behovene for å registrere anatomisk sted bestemmes i applikasjonen eller trenger større grad av kompleksitet som f.eks. relativ lokalisering, er det anbefalt å bruke CLUSTER.anatomical_location eller CLUSTER.relative_location innenfor SLOTet \"Kompleks anatomisk lokalisering\" i denne arketypen.



Dersom den anatomiske lokaliseringen inkluderes i feltet \"Problem/diagnosenavn\" via prekoordinerte koder blir dette dataelementet overflødig.">
				>
				["at0027"] = <
					text = <"Relaterte saker">
					description = <"En gruppe karakteristika som kompliserer hovedproblemet.">
				>
				["at0028"] = <
					text = <"Sak">
					description = <"Identifisering av et relatert problem eller diagnose.">
					comment = <"Registreres som tekst, kodet tekst eller som en lenke innenfor journalen.">
				>
				["at0029"] = <
					text = <"Forhold">
					description = <"Forholdet mellom problemet eller diagnosen og saken.">
				>
				["at0030"] = <
					text = <"Dato/tid for bedring">
					description = <"Estimert eller faktisk dato/tid for remisjon av det aktuelle problemet eller diagnosen, fastslått av helsepersonell.">
					comment = <"Det er mulig å registrere deldatoer.">
				>
				["at0031"] = <
					text = <"Alder ved bedring">
					description = <"Individets alder for remisjon av problemet eller diagnosen, som stadfestet av helsepersonell.">
					comment = <"Alder kan være det foretrukne dataelementet å basere beslutningsstøtte på i situasjoner der anslag av alder ikke er nøyaktig nok som grunnlag for beslutningsstøtte, f.eks. hos spedbarn under ett år.">
				>
