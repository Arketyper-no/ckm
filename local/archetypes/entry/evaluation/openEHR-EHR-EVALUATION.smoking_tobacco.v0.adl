definition
	EVALUATION[at0000] matches {	-- Tobacco smoking - Oslo
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021, 	-- Yes
									at0022, 	-- No
									at0023, 	-- Never
									at0024, 	-- Former
									at0027]	-- hgjgjgjh
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Smoking start
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Smoking pattern
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0028, 	-- regular
									at0029, 	-- somethime
									at0030]	-- seldom
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Overall use
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::382]>
								list = <
									["1"] = <
										units = <"1/d">
									>
									["2"] = <
										units = <"1/wk">
									>
									["3"] = <
										units = <"1/yr">
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Smoking end
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					CLUSTER[at0007] occurrences matches {0..*} matches {	-- Intervals
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0008] occurrences matches {0..1} matches {	-- Type
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {	-- Start
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0010] occurrences matches {0..1} matches {	-- Stop
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0011] occurrences matches {0..1} matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Triggers
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {	-- Motivation to quit
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Reason for start
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Pack Years
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Number of cessation attempts
						value matches {
							DV_COUNT matches {*}
						}
					}
					CLUSTER[at0014] occurrences matches {0..*} matches {	-- Cessation attempts
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0016] occurrences matches {0..*} matches {	-- Substitutes
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0017] occurrences matches {0..1} matches {	-- Details
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0018] occurrences matches {0..1} matches {	-- Success?
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
				}
			}
		}
	}


