definition
	EVALUATION[at0000] matches {    -- Death summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0092] occurrences matches {0..1} matches {    -- Date/time of death
						value matches {
							DV_DATE_TIME matches {*}
							DV_INTERVAL<DV_DATE_TIME> matches {*}
						}
					}
					allow_archetype CLUSTER[at0104] occurrences matches {0..*} matches {    -- Date of death alternatives
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.dod_alternative\.v0/}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Manner of death
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011,    -- Disease
									at0012,    -- Assault
									at0013,    -- Intentional self harm
									at0014,    -- Pending investigation
									at0015,    -- War
									at0016,    -- Legal intervention
									at0018]    -- Indeterminate
								}
							}
						}
					}
					ELEMENT[at0093] occurrences matches {0..1} matches {    -- Manner description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0021] occurrences matches {0..*} matches {    -- Place category
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0109] occurrences matches {0..*} matches {    -- Place of death
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0100] occurrences matches {0..*} matches {    -- Structured place of death
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.address(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {    -- Age at death
						value matches {
							DV_DURATION matches {
								value matches {PYMWDTHM/|>=PT0M|}
							}
						}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {    -- Gestation at death
						value matches {
							DV_DURATION matches {
								value matches {PWD/|P0D..P50W|}
							}
						}
					}
					ELEMENT[at0110] occurrences matches {0..1} matches {    -- Maternal context
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0112,    -- Antenatal
									at0113,    -- In labour
									at0114,    -- Postnatal
									at0115]    -- Late postnatal
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0106] occurrences matches {0..1} matches {    -- Days post partum
						value matches {
							DV_DURATION matches {
								value matches {PD/|>=P0D|}
							}
						}
					}
					allow_archetype CLUSTER[at0042] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0105] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0009] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0103] occurrences matches {0..*} matches {    -- Information source(s)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0101] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0102] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Death summary">
					description = <"Summary information about the death of an individual, excluding the cause(s) of death.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0010"] = <
					text = <"Manner of death">
					description = <"Context or setting of the death.">
					comment = <"It is recommended that 'Manner of death' be coded with an external terminology.">
				>
				["at0011"] = <
					text = <"Disease">
					description = <"">
				>
				["at0012"] = <
					text = <"Assault">
					description = <"">
				>
				["at0013"] = <
					text = <"Intentional self harm">
					description = <"">
				>
				["at0014"] = <
					text = <"Pending investigation">
					description = <"">
				>
				["at0015"] = <
					text = <"War">
					description = <"">
				>
				["at0016"] = <
					text = <"Legal intervention">
					description = <"">
				>
				["at0018"] = <
					text = <"Indeterminate">
					description = <"">
				>
				["at0021"] = <
					text = <"Place category">
					description = <"The category of the place where the individual died.">
					comment = <"This data element has multiple occurrences to allow more than one level of category, including subcategories about the place of death. It is recommended that 'Place category' be coded with an external terminology. For example: hospital or home; or, within the context of a hospital: ICU; ward; or operating theatre.">
				>
				["at0042"] = <
					text = <"Additional details">
					description = <"Additional structured details related to the death.">
				>
				["at0044"] = <
					text = <"Gestation at death">
					description = <"Number of completed weeks of gestation of the foetus at the time of foetal death.">
				>
				["at0054"] = <
					text = <"Age at death">
					description = <"The age of the individual at the time of death.">
				>
				["at0092"] = <
					text = <"Date/time of death">
					description = <"Date and/or time of death.">
					comment = <"For example: based on findings pertaining to examination of the corpse and the pathologist's reconstruction of time of death based on post-mortem changes, temperature, etc. May also be known as DOD (date of death). If more than one 'Date of death alternatives' have been proposed, this data element could be renamed in a template as the 'Confirmed/Agreed date of death'.">
				>
				["at0093"] = <
					text = <"Manner description">
					description = <"Narrative description about the context or setting of the death.">
				>
				["at0100"] = <
					text = <"Structured place of death">
					description = <"Structured detail about the place where the individual died.">
					comment = <"For example: details about a facility or institution; or a landmark or a road intersection.">
				>
				["at0101"] = <
					text = <"Last updated">
					description = <"The date when the death summary was last updated.">
				>
				["at0102"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0103"] = <
					text = <"Information source(s)">
					description = <"Narrative description about source(s) of information.">
					comment = <"For example: capturing information about one or more sources for this information, including their contribution. This may be useful if there is potentially conflicting sources of information and to enable consideration of their credibility/reliability.">
				>
				["at0104"] = <
					text = <"Date of death alternatives">
					description = <"Additional details about possible alternative dates of death.">
					comment = <"For example: Capturing information about conflicting dates and/or times of death from multiple sources with varying degree of certainty.">
				>
				["at0105"] = <
					text = <"Comment">
					description = <"Additional narrative about the death, not captured in other fields.">
					comment = <"For example: Recording the degree of certainty for place of death.">
				>
				["at0106"] = <
					text = <"Days post partum">
					description = <"The number of days from the end of a pregnancy to death.">
				>
				["at0109"] = <
					text = <"Place of death">
					description = <"The simple name of, or description about, the place where the individual died.">
					comment = <"For example: 'City Hospital'; 'Mother's house'; or 'Eiffel tower'. This data element has multiple occurrences to allow more than one level of detail about the place of death, such as recording both 'City Hospital' and 'Ward 6 North'.">
				>
				["at0110"] = <
					text = <"Maternal context">
					description = <"Timing of the death of a woman related to a pregnancy and up to one year postpartum.">
				>
				["at0112"] = <
					text = <"Antenatal">
					description = <"The woman died while pregnant and before labour.">
				>
				["at0113"] = <
					text = <"In labour">
					description = <"The woman died during labour and/or delivery.">
				>
				["at0114"] = <
					text = <"Postnatal">
					description = <"The woman died ≤42 days after the end of a pregnancy.">
				>
				["at0115"] = <
					text = <"Late postnatal">
					description = <"The woman died >42 days and ≤1 year after the end of a pregnancy.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Dødssammenfatning">
					description = <"Oppsummerende informasjon om et individs død, med unntak av dødsårsak(er).">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0010"] = <
					text = <"Klassifisering av dødsfall">
					description = <"Kontekst eller setting rundt dødsfallet.">
					comment = <"Det anbefales å kode \"Klassifisering av dødsfall\" med en ekstern terminologi.">
				>
				["at0011"] = <
					text = <"Sykdom">
					description = <"">
				>
				["at0012"] = <
					text = <"Overfall">
					description = <"">
				>
				["at0013"] = <
					text = <"Selvskading">
					description = <"">
				>
				["at0014"] = <
					text = <"Venter på undersøkelse">
					description = <"">
				>
				["at0015"] = <
					text = <"Krig">
					description = <"">
				>
				["at0016"] = <
					text = <"Rettslig inngripen">
					description = <"">
				>
				["at0018"] = <
					text = <"Ubestemmelig">
					description = <"">
				>
				["at0021"] = <
					text = <"Stedskategori">
					description = <"Stedskategori der individet døde.">
					comment = <"Dette dataelementet har multiple forekomster for å tillate mer enn ett kategorinivå, inkludert underkategorier om dødssted. Det anbefales å kode \"Stedskategori\" med en ekstern terminologi. For eksempel: sykehus eller hjemme; eller, innenfor sykehuskontekst: Intensivavdeling; sengepost; eller operasjonsstue.">
				>
				["at0042"] = <
					text = <"Ytterligere detaljer">
					description = <"Strukturerte detaljer relatert til død.">
				>
				["at0044"] = <
					text = <"Gestasjon ved død">
					description = <"Antall fullførte gestasjonsuker av fosteret ved fosterdødstidspunktet.">
				>
				["at0054"] = <
					text = <"Alder ved død">
					description = <"Individets alder ved dødstidspunktet.">
				>
				["at0092"] = <
					text = <"Dødsdato/tidspunkt">
					description = <"Dato og/eller tidspunkt for død.">
					comment = <"For eksempel: Basert på funn ved undersøkelse av liket og patologens rekonstruksjon av dødstidspunkt basert på obduksjonsfunn, temperatur, etc. Kan også være forkortet DOD (date of death). Hvis flere enn ett 'Dødsdato alternativer' foreslås, så kan dette dataelementet omdøpes i ett templat til \"Bekreftet/omforent dødsdato\".">
				>
				["at0093"] = <
					text = <"Klassifiserings beskrivelse">
					description = <"Fritekstlig beskrivelse om konteksten eller settingen rundt dødsfallet.">
				>
				["at0100"] = <
					text = <"Strukturert dødssted">
					description = <"Strukturerte detaljer om dødsstedet til individet.">
					comment = <"For eksempel: Detaljer om et anlegg eller institusjon; orienteringspunkt; eller et veikryss.">
				>
				["at0101"] = <
					text = <"Sist oppdatert">
					description = <"Dato for når dødssammendrag sist ble oppdatert.">
				>
				["at0102"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0103"] = <
					text = <"Informasjonskilder">
					description = <"Tekstlig beskrivelse av informasjonskilder.">
					comment = <"For eksempel: registrere informasjon om en eller flere informasjonskilder, inkludert bidragende informasjon. Dette kan være nyttig hvis det finnes potensielle motstridende informasjonskilder og for å kunne gjøre en vurdering av informasjonskildens kredibilitet/pålitelighet.">
				>
				["at0104"] = <
					text = <"Dødsdato alternativer">
					description = <"Ytterligere detaljer om mulige alternative dødsdato.">
					comment = <"For eksempel: Registrere informasjon fra multiple kilder om motstridende dødsdato og/eller dødstidspunkt med varierende grad av sikkerhet.">
				>
				["at0105"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om død som ikke er fanget i andre felt.">
					comment = <"For eksempel: Registrering av grad av visshet for dødssted.">
				>
				["at0106"] = <
					text = <"Dager post partum">
					description = <"Antall dager fra endt graviditet til død.">
				>
				["at0109"] = <
					text = <"Dødssted">
					description = <"Navngitt sted eller beskrivelse av stedet hvor individet døde.">
					comment = <"For eksempel: 'Lokalsykehus', 'Forelders hus'; eller 'Eiffeltårnet'. Dette dataelementet har multiple forekomster for å tillate mer enn ett detaljnivå om dødssted, som for eksempel å registrere både 'Lokalsykehus' og 'Kirurgisk sengepost nord'.">
				>
				["at0110"] = <
					text = <"Svangerskapskontekst">
					description = <"Svangerskapsrelatert dødstidspunkt av en kvinne, opp til ett år etter fødsel.">
				>
				["at0112"] = <
					text = <"Antenatal">
					description = <"Antall dager etter endt graviditet til død.">
				>
				["at0113"] = <
					text = <"Under fødsel">
					description = <"Kvinnen døde under fødsel og/eller forløsning.">
				>
				["at0114"] = <
					text = <"Postnatal">
					description = <"Kvinnen døde ≤42 dager etter endt graviditet.">
				>
				["at0115"] = <
					text = <"Sent postnatal">
					description = <"Kvinnen døde >42 dager og ≤1 år etter endt graviditet.">
				>
			>
		>
	>
