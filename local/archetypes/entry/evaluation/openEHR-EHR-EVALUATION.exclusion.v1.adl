definition
	EVALUATION[at0000] matches {	-- Eksklusjonsutsagn
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {1..*} matches {	-- Eksklusjonsutsagn
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..*} matches {	-- Spesifikt objekt
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Kommentar
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0006] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Dato for siste oppdatering
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Exclusion Statement">
					description = <"Generic, positive statement/s that need to be recorded as clinically excluded from the health record at a specific point in time.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Exclusion Statement">
					description = <"The statement about an exclusion in the health record.">
					comment = <"Use to capture any information that is needed to be positively recorded as being excluded within the record. The statement can support recording global statements or statements about a specified object. See specialisations for Medications, Adverse Reactions, Problem/Diagnosis and Procedures as common examples that have been explicitly modelled.">
				>
				["at0003"] = <
					text = <"Specific Object">
					description = <"Identification of the specific object/s to which the Exclusion Statement applies.">
				>
				["at0004"] = <
					text = <"Date Last Updated">
					description = <"The date at which the exclusion was last clinically asserted, affirmed or confirmed.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Comment">
					description = <"Additional narrative about the Exclusion not captured in other fields.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Eksklusjonsutsagn">
					description = <"Generelt eksplisitt utsagn for registrering av fravær eller eksklusjon av kliniske funn/vurderinger i journalen.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Eksklusjonsutsagn">
					description = <"Utsagn om eksklusjon i journalen.">
					comment = <"Brukes til å fange opp all informasjon som er nødvendig for eksplisitt registrering av eksklusjon i journalen. Uttalelsen støtter registrering av mer omfattende uttalelser eller uttalelser om et bestemt objekt. For eksplisitt modellerte eksempler, se spesialiseringer for legemidler, uønskede reaksjoner, problemer/diagnoser og prosedyrer.">
				>
				["at0003"] = <
					text = <"Spesifikt objekt">
					description = <"Identifikasjon av det spesifikke objektet som eksklusjonsutsagnet omhandler.">
					comment = <"Denne arketypen vil sjelden benyttes i praktisk bruk, men den er en overordnet modell i modellering av spesialiseringer hvor Spesifikt objekt kan være: Medikasjon, Problem/diagnose, Familiær forekomst, etc.">
				>
				["at0004"] = <
					text = <"Dato for siste oppdatering">
					description = <"Dato for når eksklusjonen sist ble klinisk satt, styrket eller bekreftet.">
				>
				["at0006"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0007"] = <
					text = <"Kommentar">
					description = <"Ytterligere kommentar om eksklusjonen som ikke fanges opp i andre felter.">
				>
			>
		>
	>
