							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					CLUSTER[at0050] occurrences matches {0..1} matches {	-- Vilkår ved TPH
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0046] occurrences matches {0..1} matches {	-- Pasienten har en alvorlig sinnslidelse
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
					CLUSTER[at0036] occurrences matches {0..1} matches {	-- Vilkår ved tvungen observasjon
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0047] occurrences matches {0..1} matches {	-- Det anses som overveiende sannsynlig at pasienten fyller hovedvilkåret om alvorlig sinnslidelse
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0048] occurrences matches {0..1} matches {	-- Det er overveiende sannsynlig at pasienten fyller vilkårene for tvungent psykisk helsevern etter paragraf 3-3
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0049] occurrences matches {0..1} matches {	-- Tvungen observasjon fremtrer etter en helhetsvurdering som den klart beste løsningen for pasienten
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
					CLUSTER[at0041] occurrences matches {0..1} matches {	-- Etablering av psykisk helsevern er nødvendig for å hindre at vedkommende på grunn av sinnslidelsen enten
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0042] occurrences matches {0..1} matches {	-- Får sin utsikt til helbredelse eller vesentlig bedring i betydelig grad redusert
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0043] occurrences matches {0..1} matches {	-- Med stor sannsynlighet i meget nær fremtid får sin tilstand vesentlig forverret
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0044] occurrences matches {0..1} matches {	-- Utgjør en nærliggende og alvorlig fare for eget liv og helse
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0045] occurrences matches {0..1} matches {	-- Utgjør en nærliggende og alvorlig fare for andres liv og helse
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
						}
					}
				}
			}
			ELEMENT[at0032] occurrences matches {0..1} matches {	-- Begrunnelse foreligger i journalnotat av dato
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {	-- Om iverksettelse av vedtaket
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0009, 	-- Vedtak om overføring settes ikke i verk før klagefristen er ute eller klagesaken er avgjort.
							at0010, 	-- Umiddelbar overføring er strengt nødvendig, og iverksettes. 
							at0011]	-- Det er klart at vedtaket ikke vil bli påklaget og overføringen. 
						}
					}
				}
			}
			allow_archetype CLUSTER[at0034] occurrences matches {0..1} matches {	-- Uttalerett og klageadgang
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.uttalerett_og_klageadgang(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}


