definition
	ITEM_TREE[at0000] occurrences matches {0..*} matches {	-- Tvangsform og institusjon
		items cardinality matches {1..*; unordered} matches {
			allow_archetype CLUSTER[at0030] occurrences matches {0..1} matches {	-- Vedtakstype
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.vedtakstype(-[a-zA-Z0-9_]+)*\.v1/}
			}
			CLUSTER[at0052] occurrences matches {0..1} matches {	-- Forrige tvangsform
				items cardinality matches {1..*; unordered} matches {
					allow_archetype CLUSTER[at0053] occurrences matches {0..*} matches {	-- Tvangsform
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.hjemmel(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
			allow_archetype CLUSTER[at0024] occurrences matches {0..1} matches {	-- Tvangsform
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.hjemmel(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0018] occurrences matches {0..1} matches {	-- Startdato for tvangsinnleggelsen
				value matches {
					DV_DATE matches {
						value matches {yyyy-mm-dd}
					}
				}
			}
			ELEMENT[at0021] occurrences matches {0..1} matches {	-- Utløpstidspunkt
				value matches {
					DV_DATE matches {
						value matches {yyyy-mm-dd}
					}
				}
			}
			CLUSTER[at0028] occurrences matches {0..1} matches {	-- Overføring til annen tvangsform
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Ny tvangsform er et bedre alternativ for pasienten.
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Ved overføring fra "med døgnopphold" til "uten døgnopphold" er det tatt tilbørlig hensyn til pårørende som pasienten bor sammen med. 
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- En overføring fra "uten døgnopphold" til "med døgnopphold" er et bedre alternativ for pasienten.
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
				}
			}
			CLUSTER[at0027] occurrences matches {0..1} matches {	-- Overføring til annen institusjon
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Mottakende enhet
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Institusjonen som pasienten skal overføres til er faglig og materielt i stand til å tilby pasienten tilfredsstillende behandling og omsorg og er godkjent i henhold til psykisk helsevernloven paragraf 3-5.
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
				}
			}
			CLUSTER[at0035] occurrences matches {0..1} matches {	-- Vilkår
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0037] occurrences matches {0..1} matches {	-- Hovedvilkår
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0038, 	-- Frivillig psykisk helsevern er blitt forsøkt uten at dette har ført frem
									at0039]	-- Det er åpenbart formålsløst å forsøke frivillig psykisk helsevern
								}
							}
						}
					}
					ELEMENT[at0040] occurrences matches {0..1} matches {	-- Pasienten er undersøkt av to leger, hvorav en skal være uavhengig av den ansvarlige institusjon, jfr paragraf 3-1
