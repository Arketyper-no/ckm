			purpose = <"For å strukturere journaldokumenter fra svangerskapskontroller. Dette for å sikre konsistent organisering av informasjonen samt for å fungere som en hjelp i videre fortolkning.">
			use = <"Anvendes ved svangerskapskontroller.">
			keywords = <"svangerskap", "kontroll", "prenatal", "før fødsel">
