definition
	SECTION[at0000] matches {	-- Svangerskapskontroll
		items cardinality matches {1..*; unordered} matches {
			SECTION[at0001] occurrences matches {0..1} matches {	-- Historikk
				items cardinality matches {1..*; unordered} matches {
					allow_archetype ENTRY[at0002] occurrences matches {0..*} matches {	-- Pasientens fortelling
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype OBSERVATION[at0019] occurrences matches {0..*} matches {	-- Svangerskapslenge 
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.gestation(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
			SECTION[at0003] occurrences matches {0..1} matches {	-- Funn
				items cardinality matches {1..*; unordered} matches {
					allow_archetype OBSERVATION[at0004] occurrences matches {0..1} matches {	-- Blodtrykk
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.blood_pressure(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					allow_archetype OBSERVATION[at0006] occurrences matches {0..1} matches {	-- Symfyse-fundusmålinger og fosterleie
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.exam\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					allow_archetype OBSERVATION[at0007] occurrences matches {0..1} matches {	-- Fosterbevegelser
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.fetal_movement(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					allow_archetype OBSERVATION[at0018] occurrences matches {0..*} matches {	-- Fosterhjertelyd
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.fetal_heart(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype OBSERVATION[at0008] occurrences matches {0..1} matches {	-- Urinanalyse
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.urinalysis(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype OBSERVATION[at0015] occurrences matches {0..*} matches {	-- Blodprøvesvar
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.lab_test(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype OBSERVATION[at0016] occurrences matches {0..*} matches {	-- Vekt
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.body_weight(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype OBSERVATION[at0017] occurrences matches {0..*} matches {	-- Kroppslig ødem
						include
							archetype_id/value matches {/openEHR-EHR-OBSERVATION\.exam(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
			SECTION[at0011] occurrences matches {0..1} matches {	-- Notater
				items cardinality matches {1..*; unordered} matches {
					allow_archetype ENTRY[at0012] occurrences matches {0..*} matches {	-- Vudering
						include
							archetype_id/value matches {/openEHR-EHR-EVALUATION\.clinical_synopsis(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
			SECTION[at0009] occurrences matches {0..1} matches {	-- Neste kontroll
				items cardinality matches {1..*; unordered} matches {
					allow_archetype INSTRUCTION[at0010] occurrences matches {0..*} matches {	-- Svangerskapskontroll
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


