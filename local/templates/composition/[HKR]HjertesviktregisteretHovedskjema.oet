  <annotations path="[openEHR-EHR-COMPOSITION.encounter.v1]/content[openEHR-EHR-SECTION.adhoc.v1 and name/value='Basisdel']/items[openEHR-EHR-OBSERVATION.enkelt_svar_med_forklaring.v1 and name/value='Hovedårsak til hjertesvikt']/data[at0001]/events[at0002]/data[at0003]/items[at0004]">
