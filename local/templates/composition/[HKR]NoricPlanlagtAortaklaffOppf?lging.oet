  <annotations path="[openEHR-EHR-COMPOSITION.encounter.v1]/content[openEHR-EHR-SECTION.adhoc.v1 and name/value='Aktuell preoperativ status']/items[openEHR-EHR-OBSERVATION.enkelt_svar_med_forklaring.v1 and name/value='NYHA klasse']/data[at0001]/events[at0002]/data[at0003]/items[at0004]">
    <items>
