  <annotations path="[openEHR-EHR-COMPOSITION.encounter.v1]/content[openEHR-EHR-SECTION.adhoc.v1 and name/value='Livskvalitet iflg EQ-5D']/items[openEHR-EHR-OBSERVATION.enkelt_svar_med_forklaring.v1 and name/value='Aktiviteter']/data[at0001]/events[at0002]/data[at0003]/items[at0004]">
