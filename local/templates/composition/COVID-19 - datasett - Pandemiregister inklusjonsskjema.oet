        name="COVID-19 - datasett - Pandemiregister inklusjonsskjema"
        xmlns:tem="openEHR/v1/Template"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="tem:COMPOSITION">
        <Content archetype_id="openEHR-EHR-SECTION.adhoc.v1"
            concept_name="Templat-overskrift" max="1" min="1"
            name="Inklusjon" path="/content" xsi:type="tem:SECTION">
            <Item
                archetype_id="openEHR-EHR-ADMIN_ENTRY.episode_institution.v0"
                max="1" min="1" path="/items" xsi:type="tem:ADMIN_ENTRY">
                <Rule min="1"
                    name="Tidspunkt for innleggelse ved denne institusjonen" path="/data[at0001]/items[at0004]"/>
                <Rule min="1"
                    name="Hvor kom pasienten fra ved innleggelse i dette sykehuset?" path="/data[at0001]/items[at0007]">
                    <constraint xsi:type="tem:textConstraint">
                        <includedValues>SNOMED-CT::22232009::Hospital (environment)</includedValues>
                        <includedValues>SNOMED-CT::285202004::Community environment (environment)</includedValues>
                        <includedValues>SNOMED-CT::261665006::Unknown (qualifier value)</includedValues>
                    </constraint>
                </Rule>
                <Rule max="0" path="/data[at0001]/items[at0002]"/>
                <Rule max="0" path="/data[at0001]/items[at0006]"/>
                <Rule max="0" path="/data[at0001]/items[at0003]"/>
            </Item>
            <Item
                archetype_id="openEHR-EHR-EVALUATION.problem_diagnosis.v1"
                concept_name="Problem/diagnose" max="1" min="1"
                name="Diagnose" path="/items" xsi:type="tem:EVALUATION">
                <Rule path="/data[at0001]/items[at0002]">
                    <constraint xsi:type="tem:textConstraint">
                        <includedValues>SNOMED-CT::840539006::Disease caused by severe acute respiratory syndrome coronavirus 2 (disorder)</includedValues>
                    </constraint>
                </Rule>
